      - text: "."
  Npc_oasis023_012:
    attributes: Npc_oasis023
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "La foudre que fait tomber cette créature\nn'atteint pas le "
      - control:
          kind: set_colour
          colour: red
      - text: "poste d'observation"
      - control:
          kind: reset_colour
      - text: ", mais\nles monstres restent nombreux par là.\nPersonnellement, je préfère m'en tenir\néloignée."
