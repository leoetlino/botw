entries:
  "0000":
    attributes: ""
    contents:
      - text: "J'ai vu son âme."
  "0002":
    attributes: ""
    contents:
      - text: "C'est promis."
  "0003":
    attributes: ""
    contents:
      - text: Elle est avec moi.
  talk00:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: Link !
      - control:
          kind: pause
          frames: 10
      - text: " Je te remercie du fond du cœur\nd'avoir sauvé notre village !\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Tout le monde au domaine s'en réjouit !"
      - control:
          kind: pause
          frames: 20
      - text: "\nNous n'avons jamais été plus heureux !"
  talk01:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: "Toutefois, Link..."
      - control:
          kind: pause
          frames: 20
      - text: " Il y a une question que\nj'aimerais te poser.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Tu es revenu sain et sauf des entrailles\nde "
      - control:
          kind: set_colour
          colour: blue
      - text: "Vah'Ruta"
      - control:
          kind: reset_colour
      - text: ", la créature divine de\nl'eau...\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Mais, Mipha, elle, n'est pas revenue...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 16
      - text: Dois-je en déduire que...
      - control:
          kind: pause
          frames: 20
      - text: " qu'il était déjà\ntrop tard ?"
      - control:
          kind: single_choice
          label: 0
  talk02:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 6
      - text: "Son... Son âme...\n\n\nEt, a-t-elle..."
      - control:
          kind: pause
          frames: 20
      - text: " Mipha a-t-elle dit\nquelque chose ?"
  talk04:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "... Je vois... Mipha..."
      - control:
          kind: pause
          frames: 20
      - text: " Quelle tragédie...\n\n\nEt je n'ai rien pu faire...\n\n\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Mais...
      - control:
          kind: pause
          frames: 20
      - text: " Mipha accomplit son devoir de\nProdige.\n\nDans ce cas... "
      - control:
          kind: pause
          frames: 20
      - text: "mon rôle de père est de\nveiller sur elle...\n\n..."
      - control:
          kind: pause
          frames: 22
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Link...
      - control:
          kind: pause
          frames: 20
      - text: " Elle t'aimait énormément.\n\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Et je suis certain qu'en ce moment même\nelle est heureuse de pouvoir te venir en\naide.\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 6
      - text: "Je t'en prie, garde toujours le souvenir\nde Mipha."
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
          selected_index: 0
          cancel_index: 2
          unknown: 6
  talk05:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Je t'en remercie... !"
      - control:
          kind: pause
          frames: 20
      - text: " Tu es vraiment\nquelqu'un de bien !"
  talk06:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Tes paroles me font chaud au\ncœur !"
      - control:
          kind: pause
          frames: 20
      - text: " Merci infiniment !"
  talk07:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 4
      - text: "Link, si tu as besoin de quoi que ce soit,"
      - control:
          kind: pause
          frames: 20
      - text: "\nn'hésite pas à venir au domaine.\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Tous mes sujets t'accueilleront les bras\nouverts !\n\nMerci, Link..."
      - control:
          kind: pause
          frames: 20
      - text: " Merci."
  talk08:
    attributes: Npc_Zora003
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Merci, Link."
      - control:
          kind: pause
          frames: 20
      - text: " Tu nous as sauvés !\n\n\nJe suis certain que Mipha est heureuse\nde pouvoir être auprès de toi.\n\nJe veillerai sur vous deux d'ici pendant\nque vous combattrez côte à côte !"
