          unknown:
            - 3
            - 12
      - text: Ah !
      - control:
          kind: pause
          length: long
      - text: " Il en faudra plus pour m'abattre !"
  Failed01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Ouille,"
      - control:
          kind: pause
