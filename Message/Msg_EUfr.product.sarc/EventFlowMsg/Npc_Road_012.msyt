  SavedLv3_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "C'était chaud... "
      - control:
          kind: pause
          length: short
      - text: "Ç'aurait pu mal tourner..."
