      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: "Hé, ce serait pas un orbe ? Me dis pas\nque c'est l'"
      - control:
          kind: set_colour
          colour: red
      - text: orbe sacré
      - control:
          kind: reset_colour
      - text: " des Gerudos ?\n\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Je rêve ! On l'avait vu avant !"
  Npc_Attacked_011_DLC2nd_SD_200:
    attributes: Npc_Attacked_011_DLC2nd
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Non mais, t'es pas bien ?! Où t'as appris\nà jeter des trésors aux orties comme ça ?\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 13
      - text: "Si tu le voulais pas, t'aurais pu me le\ndonner, tu sais..."
