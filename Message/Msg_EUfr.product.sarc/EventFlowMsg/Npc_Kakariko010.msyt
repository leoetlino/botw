    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "J'ai tout vu, mon garçon !"
      - control:
          kind: pause
          length: longer
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n\nTu as été épatant !"
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Cela faisait si longtemps que je n'avais\nrien vu de pareil..."
      - control:
          kind: pause
          length: short
      - text: "\n\n"
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - control:
          kind: text_size
          percent: 100
      - text: "Mon mari est un excellent archer, lui\naussi..."
      - control:
          kind: pause
          length: short
      - text: " Mais nous faisons une sorte\nde..."
      - control:
          kind: pause
          length: short
      - text: " pause, tous les deux.\nMalgré tout,"
      - control:
          kind: pause
          length: short
      - text: " quelle émotion exquise !"
      - control:
          kind: pause
          length: longer
      - text: "\nCela faisait si longtemps !"
      - control:
