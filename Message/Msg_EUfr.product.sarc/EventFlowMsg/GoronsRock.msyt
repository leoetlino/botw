entries:
  "0000":
    attributes: ""
    contents:
      - text: Ton frangin ?
  "0001":
    attributes: ""
    contents:
      - text: "Il s'est enfui ?"
  "0002":
    attributes: ""
    contents:
      - text: Où est-il allé ?
  "0003":
    attributes: ""
    contents:
      - text: Le secret du Héros ?
  "0004":
    attributes: ""
    contents:
      - text: Je le comprends !
  "0005":
    attributes: ""
    contents:
      - text: Le tunnel des quoi ?
  Goron_Father_Near01:
    attributes: Npc_Goron010
    contents:
      - text: À la CASSEROLE !
  Goron_Father_Near02:
    attributes: Npc_Goron010
    contents:
      - text: Que peut-il bien FAIRE ?
  Goron_Father_Talk01:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mon AGITÉ de FRANGIN n'est toujours\npas rentré !\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Mais à quoi il pense, à la FIN ?!"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk02:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Enfin je te COMPRENDS, mon frère !"
  Goron_Father_Talk03:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tu as fait ce qu'il fallait..."
  Goron_Father_Talk04:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Je dois te demander une faveur... Je ne\npeux rester sans rien faire alors qu'il a\nBESOIN de moi !"
  Goron_Father_Talk05:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Je vais tout préparer pour être prêt à\ncuisiner à ton arrivée. Je t'attendrai ici !"
  Goron_Father_Talk06:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: MERVEILLEUX !!!
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nCe "
      - control:
          kind: set_colour
          colour: red
      - text: "gigot de caillasse "
      - control:
          kind: reset_colour
      - text: "fera PARFAITEMENT\nl'affaire ! Je vais me mettre TOUT DE\nSUITE à la cuisine !"
  Goron_Father_Talk06_2:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: "Et VOILÀ ! Un "
      - control:
          kind: set_colour
          colour: red
      - text: "DÉLICIEUX gigot de\ncaillasse rôti "
      - control:
          kind: reset_colour
      - text: "!"
  Goron_Father_Talk07:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 1
            - 205
      - text: "C'est PRÊT, frérot ! À TABLE !"
  Goron_Father_Talk08:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound2
          unknown:
            - 2
            - 205
      - text: "C'était donc ça, le "
      - control:
          kind: set_colour
          colour: red
      - text: "SECRET du Héros "
      - control:
          kind: reset_colour
      - text: "?\n\n\n"
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "C'est drôle, ça me rappelle quelque chose..."
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n\n"
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Quoi qu'il en soit, FÉLICITATIONS,\nfrérot ! Ton grand frère est très FIER !"
  Goron_Father_Talk09:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Merci d'avoir AIDÉ mon FRÈRE !\n\n\nNous allons rentrer au village, à présent."
  Goron_Father_Talk10:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Quand mon FRANGIN va-t-il se DÉCIDER\nà RENTRER ?!\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Je ne peux pas croire qu'il soit encore\ndans le "
      - control:
          kind: set_colour
          colour: blue
      - text: tunnel de Marpo
      - control:
          kind: reset_colour
      - text: " à tenter\nde déterrer le "
      - control:
          kind: set_colour
          colour: red
      - text: secret du Héros
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk11:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Le "
      - control:
          kind: set_colour
          colour: blue
      - text: tunnel de Marpo
      - control:
          kind: reset_colour
      - text: ", un tunnel\nabandonné au sud-ouest de l'"
      - control:
          kind: set_colour
          colour: blue
      - text: "étang\nde Marpo"
      - control:
          kind: reset_colour
      - text: ".\nLa roche est si dure au fond de la galerie\nque nous avons arrêté l'exploitation de la\nzone il y a bien longtemps.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Et il semblerait que ce soit derrière cette\nroche que se cache le "
      - control:
          kind: set_colour
          colour: red
      - text: secret du Héros
      - control:
          kind: reset_colour
      - text: "..."
  Goron_Father_Talk12:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Quand tu sors d'ici, "
      - control:
          kind: set_colour
          colour: red
      - text: descends la colline
      - control:
          kind: reset_colour
      - text: "\net tu arriveras aux falaises de Wagoro.\nLà-bas on trouve du "
      - control:
          kind: set_colour
          colour: red
      - text: gigot de caillasse
      - control:
          kind: reset_colour
      - text: ".\nSois gentil et ramène-m'en un bien\nrocailleux !"
  Goron_Father_Talk13:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Je ne t'embêterais pas avec ça si je\npouvais y aller moi-même...\n\nMalheureusement, avec ma goronite\nchronique de l'épaule interne, c'est\ntout à fait déconseillé.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Mais TOI, tu m'as tout l'air d'être à la\nHAUTEUR de la tâche ! Tu veux bien\nm'aider, n'est-ce pas ?"
  Goron_Father_Talk14:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Bégron, mon frère ADORÉ, la PRUNELLE\nde mes yeux !\n\n"
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - text: "Il a dit qu'il partait déterrer le "
      - control:
          kind: set_colour
          colour: red
      - text: "secret du\nHéros"
      - control:
          kind: reset_colour
      - text: "... et HOP LÀ ! Disparu !\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Je ne comprends pas ce qui l'a pris !"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk15:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "S'il avait l'intention de S'ENFUIR,\npourquoi irait-il raconter qu'il veut\ndéterrer le "
      - control:
          kind: set_colour
          colour: red
      - text: SECRET du Héros
      - control:
          kind: reset_colour
      - text: " ?!\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Aah, Bégron... Qu'est-ce que tu es allé\nte mettre dans la tête ?"
      - control:
          kind: choice
          choice_labels:
            - 2
            - 3
            - 4
          selected_index: 0
          cancel_index: 3
          unknown: 8
  Goron_Father_Talk16:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Au "
      - control:
          kind: set_colour
          colour: blue
      - text: tunnel de Marpo
      - control:
          kind: reset_colour
      - text: ", au sud-ouest\nde l'"
      - control:
          kind: set_colour
          colour: blue
      - text: étang de Marpo
      - control:
          kind: reset_colour
      - text: ", j'en suis sûr.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "La roche au fond du tunnel est si dure\nque nous avons cessé d'y creuser il y a\nbien longtemps..."
  Goron_Father_Talk17:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Quand tu auras trouvé un "
      - control:
          kind: set_colour
          colour: red
      - text: "gigot de\ncaillasse"
      - control:
          kind: reset_colour
      - text: ", balance-le sans façons\ndans mon plat de cuisson !"
  Goron_Father_Talk18:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Mais je ne suis PAS DUPE ! Je sais bien\nque ce n'est qu'une EXCUSE pour se la\nCOULER DOUCE !"
  Goron_Father_Talk20:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Au sud-ouest de l'"
      - control:
          kind: set_colour
          colour: blue
      - text: étang de Marpo
      - control:
          kind: reset_colour
      - text: ",\nil y a un tunnel abandonné qu'on appelle\nle "
      - control:
          kind: set_colour
          colour: blue
      - text: tunnel de Marpo
      - control:
          kind: reset_colour
      - text: ".\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Et caché dans sa roche aussi DURE que\nle MÉTAL, le "
      - control:
          kind: set_colour
          colour: red
      - text: "SECRET du Héros "
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "C'est ce que m'a raconté le voisin du\ncousin par alliance de mon grand-père,\nen tout cas."
  Goron_Father_Talk21:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: "Ah, je t'ATTENDAIS ! Allez hop, dans\nle PLAT et plus VITE que ça !"
  Goron_Father_Talk22:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Le "
      - control:
          kind: set_colour
          colour: red
      - text: secret du Héros
      - control:
          kind: reset_colour
      - text: " n'est rien qu'une\nvieille histoire qu'on se raconte entre\nGorons.\nJe l'ai moi-même entendue du voisin du\ncousin par alliance de mon grand-père... "
  Goron_Father_Talk23:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: QUOI ?!
      - control:
          kind: pause
          length: short
      - text: " Tu le COMPRENDS ?\nAlors tu SAIS ce qu'il a dans la tête ?!\n\n"
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Et moi, son propre frère, j'en suis totalement\nincapable... Quelle tristesse..."
      - control:
          kind: text_size
          percent: 100
  Goron_Father_Talk24:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Je ne sais plus quoi faire de lui. Pourquoi\nn'essaye-t-il pas de se rendre utile à la\nmine au lieu de rêver ?"
  Goron_Son_Near01:
    attributes: Npc_Goron009
    contents:
      - text: Le secret du Héros !
  Goron_Son_Near02:
    attributes: Npc_Goron009
    contents:
      - text: "J'ai FAIM !"
  Goron_Son_Talk01:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - text: "Faut creuser, creuser et toujours\ncreuser ! Le "
      - control:
          kind: set_colour
          colour: red
      - text: "secret "
      - control:
          kind: reset_colour
      - text: "est CACHÉ LÀ !\n\nPourquoi je creuse comme ça ? Peux\npas le dire, c'est un "
      - control:
          kind: set_colour
          colour: red
      - text: "SECRET "
      - control:
          kind: reset_colour
      - text: "!"
  Goron_Son_Talk02:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: "Fiou... "
      - control:
          kind: pause
          length: long
      - text: "J'en peux plus...\n\n\nDerrière tout ce roc, c'est là qu'est\ncaché le "
      - control:
          kind: set_colour
          colour: red
      - text: "secret du HÉROS "
      - control:
          kind: reset_colour
      - text: "!\n\nAh, si je pouvais mettre la main dessus...\nOn me respecterait, ÇA OUI !\n\n"
      - control:
          kind: sound2
          unknown:
            - 40
            - 205
      - text: "Mais moi, j'arrive plus à creuser...\nJe me sens tout faible,"
      - control:
          kind: pause
          length: short
      - text: " parce que\nj'ai trop FAIM !"
  Goron_Son_Talk02b:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 43
      - text: Peux même plus lever mes bras...
  Goron_Son_Talk04:
    attributes: Npc_Goron009
    contents:
      - text: "Miam, c'est DIVIN ! DIVIN ! Miam...\nCrounch crounch miam...\n\nCes minéraux croustillants sur la surface\nbien saisie de la CAILLASSE ! AAH ! Plus\nje mange et plus j'en VEUX !\nCe bon jus de pierre qui vous BRÛLE\nles gencives ! Mouah ah AH ! EXQUIS !\n\nMes forces me reviennent, je suis GRAND\net FORT comme la montagne de la Mort...\nGRRAAAAAH !"
  Goron_Son_Talk04_1:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Oh ? Je reconnaîtrais ce fumet entre\nmille... un "
      - control:
          kind: set_colour
          colour: red
      - text: "délicieux gigot de caillasse\nRÔTI "
      - control:
          kind: reset_colour
      - text: "!"
  Goron_Son_Talk04_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Regarde-moi ÇA ! J'en reviens PAS !\nQuel festin, je le veux, je le VEUX !\nJ'ai trop FAIM !!!"
  Goron_Son_Talk04_3:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound2
          unknown:
            - 9
            - 205
      - text: "Personne ne cuisine aussi bien que TOI !\nHAHA !"
  Goron_Son_Talk04_4:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "C'est fini, je n'ai plus faim. Mettons-nous\nau TRAVAIL !"
  Goron_Son_Talk05:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - control:
          kind: text_size
          percent: 125
      - text: Ouuuaaaah !!!
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk05_2:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 10
      - control:
          kind: text_size
          percent: 125
      - text: TORNAAADE GORON !
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk06:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - control:
          kind: animation
          name: Happy_00
      - text: "Ça y est, le VOILÀ ! Le seul et unique\n"
      - control:
          kind: set_colour
          colour: red
      - text: trésor du HÉROS
      - control:
          kind: reset_colour
      - text: " !\n\n"
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - control:
          kind: text_size
          percent: 80
      - text: "Curieux, j'ai l'impression d'avoir déjà vu un\ntruc comme ça au village Goron..."
      - control:
          kind: text_size
          percent: 100
  Goron_Son_Talk07:
    attributes: Npc_Goron009
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 0
      - text: "Merci, l'AMI ! Je vais rentrer au\nvillage Goron et raconter ma\ndécouverte à TOUT LE MONDE !"
