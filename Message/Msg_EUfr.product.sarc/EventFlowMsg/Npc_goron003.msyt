          kind: sound
          unknown:
            - 6
            - 0
      - text: "J'me baladais tout à l'heure près d'la\n"
      - control:
          kind: set_colour
          colour: blue
      - text: cime de la montagne de la Mort
      - control:
          kind: reset_colour
