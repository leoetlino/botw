          unknown: 6
  talk_05:
    attributes: HorseGod001
    contents:
      - text: "Eh oui, il n'est jamais facile de se dire\nadieu... Quel cheval veux-tu que je\nlibère ?"
  talk_06:
    attributes: HorseGod001
    contents:
