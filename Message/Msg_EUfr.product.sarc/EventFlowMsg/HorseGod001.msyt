  talk_03_00:
    attributes: HorseGod001
    contents:
      - text: "Vraiment ? "
      - control:
          kind: pause
          length: short
      - text: "Ainsi, la mort de ton ami\nfidèle ne te fait ni chaud ni froid ?"
  talk_03_01:
    attributes: HorseGod001
    contents:
      - control:
          kind: text_size
          percent: 125
      - control:
          kind: auto_advance
          frames: 60
      - control:
