            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk_12_00:
    attributes: HorseGod001
    contents:
      - control:
          kind: sound2
          unknown:
            - 14
            - 205
      - text: "Bien, je vais donc ramener ce cheval à\nla vie !\n\nOh ? "
      - control:
          kind: pause
          length: short
      - text: "Une seconde...\n\n\nQUOI ? "
      - control:
          kind: pause
          length: long
      - text: "Cette pauvre bête me dit qu'elle\nest morte de ta main ! "
      - control:
          kind: pause
          length: long
