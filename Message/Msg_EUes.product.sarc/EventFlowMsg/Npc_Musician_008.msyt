  Talk15:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 8
      - text: Aguardad un momento...
      - control:
          kind: pause
          length: short
      - text: " Ese artefacto\nque lleváis a la cadera..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "Disculpad, "
      - control:
          kind: pause
          length: long
      - text: "no es nada.\nNo debería molestaros cuando\nacabamos de conocernos."
      - control:
