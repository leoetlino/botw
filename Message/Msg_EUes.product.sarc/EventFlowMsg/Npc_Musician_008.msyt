  Talk00:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 19
      - text: "Decidme, ¿me haríais el honor\nde escuchar una canción?"
      - control:
          kind: choice
          choice_labels:
            - 0
