    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Heh. "
      - control:
          kind: pause
          length: short
      - text: "People have always noted how\nwe're nothing alike, even though\nwe're sisters.\n"
      - control:
