      - control:
          kind: set_colour
          colour: blue
      - text: Hyrule Castle
      - control:
          kind: reset_colour
      - text: ".\nAs for Robbie, "
      - control:
          kind: pause
          length: short
      - text: "he went on to adapt his\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Guidance Stone "
