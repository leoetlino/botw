      - control:
          kind: reset_colour
      - text: "in\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Hateno Village
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "is the energy source\nneeded to operate the "
      - control:
          kind: set_colour
          colour: blue
      - text: Guidance Stone
      - control:
          kind: reset_colour
      - text: ".\nThe "
      - control:
          kind: set_colour
          colour: blue
      - text: "Guidance Stone "
      - control:
          kind: reset_colour
      - text: "at my lab "
      - control:
          kind: pause
