          length: short
      - text: " this venture will\nbe very profitable for you as well!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "You see, "
      - control:
          kind: set_colour
          colour: red
      - text: "Prince Sidon "
      - control:
          kind: reset_colour
      - text: "of the Zora "
      - control:
          kind: pause
          length: short
      - text: "is in\ndesperate search of a strong Hylian.\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "And as this is a royal request from the\n"
      - control:
          kind: set_colour
          colour: red
      - text: "prince "
      - control:
          kind: reset_colour
      - text: "himself, "
      - control:
          kind: pause
          length: short
