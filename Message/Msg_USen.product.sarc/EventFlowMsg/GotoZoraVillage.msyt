  Npc_Lanayru003_R_011:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Since you've got a sword and stuff,\nyou'll probably be fine.\n\nJust watch out for a creepy guy calling\nhimself a "
      - control:
          kind: set_colour
          colour: red
      - text: prince
      - control:
          kind: reset_colour
      - text: "."
  Npc_Lanayru003_R_012:
    attributes: Npc_Lanayru003
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Yeah, I wouldn't head that way."
      - control:
          kind: pause
          length: short
      - text: "\nI just came from there, and I ran into\nthis shady guy calling himself a "
      - control:
          kind: set_colour
          colour: red
      - text: prince
      - control:
          kind: reset_colour
      - text: ".\nThis world we live in..."
