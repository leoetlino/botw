    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Hyrule Castle "
      - control:
          kind: reset_colour
      - text: "was the governing center\nof Hyrule, home to the king and the\nbeautiful princess.\nBrave souls from nearby villages would\nall set their sights there, do great deeds,\nand return home draped in glory.\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: Hyrule Castle
      - control:
          kind: reset_colour
