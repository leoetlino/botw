      - control:
          kind: sound
          unknown:
            - 8
            - 0
      - text: "Be on your best manners. If she deems\nyou to be a disturbance to her, it will not\nend well for you!"
  Npc_oasis007_Strategy_001:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: Lady Riju
      - control:
          kind: reset_colour
      - text: " could tell who you were—what\nyou were capable of—all along."
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "\n\nI thought I was a good judge of\ncharacter, "
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "but clearly I still have a ways\nto go.\nLeave the town's defenses to me.\n"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 8
            - 1
      - text: "Lady Riju "
      - control:
          kind: reset_colour
      - text: "needs you at the "
      - control:
          kind: set_colour
          colour: red
      - text: lookout post
      - control:
          kind: reset_colour
      - text: "."
