  Npc_oasis003_Strategy_027:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 14
      - text: "...Wait."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\nYou don't have a lot of "
      - control:
          kind: set_colour
          colour: blue
      - text: bomb arrows
      - control:
          kind: reset_colour
      - text: ".\nAre you really planning on going out\nlike that?\nThese were basically my emergency,\nemergency supply. I entrust them to you,\nbut that really is all I've got!"
  Npc_oasis004_GetBack_001:
    attributes: Npc_GerudoDesert004
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: "Hey, what are you doing here?"
      - control:
          kind: pause
          length: short
      - text: " This is\nthe hideout of those Yiga thieves who\nstole the "
      - control:
          kind: set_colour
          colour: red
      - text: Thunder Helm
      - control:
          kind: reset_colour
