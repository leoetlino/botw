  Npc_oasis003_Strategy_020:
    attributes: Npc_oasis003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ancestors of the Gerudo!\nAnswer my call! Aid me in this task!"
