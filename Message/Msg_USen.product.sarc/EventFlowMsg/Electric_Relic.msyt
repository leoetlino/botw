    attributes: Npc_GerudoDesert004
    contents:
      - text: Gotta escape somehow...
  Npc_oasis007_Defeat_001:
    attributes: Npc_oasis007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "I was so worried that something horrible\nwould happen to the "
      - control:
          kind: set_colour
          colour: blue
      - text: chief
      - control:
          kind: reset_colour
      - text: ", I could barely\ncontain myself.\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
