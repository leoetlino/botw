  talk15:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Business, eh?\n\n\nIn the old days, you could easily find\n"
      - control:
          kind: set_colour
          colour: blue
      - text: jewels
      - control:
          kind: reset_colour
