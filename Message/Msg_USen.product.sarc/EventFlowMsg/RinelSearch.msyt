  talk04:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Prince Sidon ran off through the\neastern gate. "
      - control:
          kind: pause
          length: short
      - text: "He was shouting\nsomething like..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "\"I'm off to stop the Divine Beast!\nLink the Hylian is coming with me!\"\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "I assume that means you're going\nup "
      - control:
          kind: set_colour
          colour: blue
      - text: Ploymus Mountain
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: " in search of\n"
      - control:
          kind: set_colour
          colour: blue
      - text: shock arrows
      - control:
