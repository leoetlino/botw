  Npc_oasis004_116:
    attributes: NPC_oasis004
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "I really appreciate what you've done so\nfar, but do you think you could find all\nfour of my friends?\n"
