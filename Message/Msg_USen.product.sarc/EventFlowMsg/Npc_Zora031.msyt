            - 16
      - text: "Oh... "
      - control:
          kind: pause
          length: short
      - text: "But, Master Link, "
      - control:
          kind: pause
          length: short
      - text: "you probably\nshouldn't speak to the elderly.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Oops! "
      - control:
          kind: pause
          length: short
      - text: "Sorry for keeping you too long.\n\n\nYou have business in the "
      - control:
          kind: set_colour
          colour: red
