          kind: reset_colour
      - text: " just start\nglowing?"
      - control:
          kind: pause
          length: long
      - text: " I suppose that proves\nI didn't imagine it the first time."
  Npc_Musician_AoC_HeroXxxx_Demo610_030:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "This "
      - control:
          kind: set_colour
          colour: blue
      - text: stone monument
      - control:
          kind: reset_colour
      - text: " has survived\nfor 10,000 years."
      - control:
          kind: pause
          length: long
      - text: " What secrets does\nit hold..."
      - control:
          kind: pause
          length: long
