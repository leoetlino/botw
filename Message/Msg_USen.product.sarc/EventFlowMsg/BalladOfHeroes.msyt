          cancel_index: 2
          unknown: 6
  Npc_Musician_AoC_HeroXxxx_Demo610_First_030:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Should we ever meet at a location\nmentioned in an "
      - control:
          kind: set_colour
          colour: blue
      - text: ancient song
      - control:
          kind: reset_colour
      - text: ",\nI would love to sing for you.\nOh... I almost forgot."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_050:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Yes. The song is mostly complete.\nHowever..."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_060:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "My teacher left this world without\nfinishing the "
      - control:
          kind: set_colour
          colour: blue
      - text: key verses
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "I wish to weave the missing pieces\ntogether and finish the song.\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "That's why I'm roaming the areas\nmentioned in the lyrics."
