  Npc_Musician_AoC_HeroXxxx_Demo610_First_010:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 8
      - text: Hm?
      - control:
          kind: pause
          length: longer
      - text: " That thing on your waist..."
      - control:
