  Obj_BombArrow_A_02_Desc:
    contents:
      - text: "A bundle of five bomb arrows.\nThe explosive powder packed into the tips\nof these arrows ignites on impact, dealing\nbig damage to anything caught in the blast."
  Obj_BombArrow_A_02_Name:
    contents:
      - text: Bomb Arrow x5
  Obj_BombArrow_A_02_PictureBook:
    contents:
      - text: "A bundle of five bomb arrows.\nThe explosive powder packed into the tips\nof these arrows ignites on impact, dealing\nbig damage to anything caught in the blast."
  Obj_BombArrow_A_03_Desc:
    contents:
      - text: "A bundle of 10 bomb arrows.\nThe explosive powder packed into the tips\nof these arrows ignites on impact, dealing\nbig damage to anything caught in the blast."
  Obj_BombArrow_A_03_Name:
    contents:
      - text: Bomb Arrow x10
  Obj_BombArrow_A_03_PictureBook:
    contents:
      - text: "A bundle of 10 bomb arrows.\nThe explosive powder packed into the tips\nof these arrows ignites on impact, dealing\nbig damage to anything caught in the blast."
  Obj_BombArrow_A_04_Desc:
    contents:
      - text: "A bundle of 20 bomb arrows.\nThe explosive powder packed into the tips\nof these arrows ignites on impact, dealing\nbig damage to anything caught in the blast."
  Obj_BombArrow_A_04_Name:
    contents:
      - text: Bomb Arrow x20
  Obj_BombArrow_A_04_PictureBook:
    contents:
      - text: "A bundle of 20 bomb arrows.\nThe explosive powder packed into the tips\nof these arrows ignites on impact, dealing\nbig damage to anything caught in the blast."
  Obj_BombArrow_A_10_Desc:
    contents:
      - text: "A powerful arrow designed to destroy\nmonsters. The explosive powder packed into\nthe tip ignites on impact, dealing massive\ndamage to anything caught in the blast."
  Obj_BombArrow_A_10_Name:
    contents:
      - text: Bomb Arrow x
      - control:
          kind: variable
          variable_kind: 14
