          label: 10
  Talk_204:
    attributes: Npc_oasis037
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Meissie...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Zeg me niet dat je niet weet wie ik ben...
  Talk_205:
    attributes: Npc_oasis037
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "Wat?! En van 'Traysi's Tamtam' heb\nje ook nog nooit gehoord?"
