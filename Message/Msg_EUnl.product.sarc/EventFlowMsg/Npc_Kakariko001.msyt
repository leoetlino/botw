  Anataha00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
      - text: Wat is er?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Je kijkt alsof je me nog nooit eerder hebt\ngezien. Die ogen..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik ben het, Impa..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Je moet je toch op z'n minst mijn naam\nkunnen herinneren, of niet?"
  Anataha01:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - control:
          kind: text_size
          percent: 125
      - text: Maar!
      - control:
          kind: text_size
          percent: 100
  Anataha02:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "De boodschap die de prinses voor je\nheeft achtergelaten..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "Als ik je hem laat horen, betekent dat \ndat je vanaf dat moment je leven op het\nspel moet zetten."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Maar... dat kan ik niet van je vragen\nterwijl je je geheugen kwijt bent."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Kom terug zodra je er klaar voor bent om\nde boodschap van de prinses te horen."
  Anataha03:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - text: "Je... "
      - control:
          kind: pause
          length: short
      - text: Je bent echt je geheugen kwijt...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 16
      - text: "Nou ja, "
      - control:
          kind: pause
          length: short
      - text: "op dit moment is dat misschien\nmaar goed ook."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Link...
      - control:
          kind: pause
          length: longer
      - text: " Kom een beetje dichterbij."
  Anataha06:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Honderd jaar geleden...
  Anataha07:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Ja...
      - control:
          kind: pause
          length: short
      - text: " Het was honderd jaar geleden dat\nhet koninkrijk Hyrule werd verwoest..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het was de laatste wens van prinses\nZelda om jou, nadat je was verslagen, in\neen diepe slaap te brengen.\nEn vervolgens bond ze in haar eentje..."
      - control:
          kind: pause
          length: short
      - text: "\nin haar eentje de strijd aan met "
      - control:
          kind: set_colour
          colour: red
      - text: Ganon
      - control:
          kind: reset_colour
      - text: "."
  Anataha08:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: Prinses Zelda...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ze liet een "
      - control:
          kind: set_colour
          colour: red
      - text: speciale boodschap
      - control:
          kind: reset_colour
      - text: " voor je\nachter..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik heb honderd jaar gewacht om je die\nboodschap te geven..."
  CarryingBlueFireEXMini02:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: Huh?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Ik hoorde dat je van Purah van hot naar\nher moest rennen, maar je ziet er prima\nuit."
  CarryingBlueFireEXMini03:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Hm...
      - control:
          kind: pause
          length: long
      - text: " Laat mij eens kijken."
  CarryingBlueFireEXMini05:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - text: Ah...
      - control:
          kind: pause
          length: long
      - text: " Dit is zonder twijfel een "
      - control:
          kind: set_colour
          colour: red
      - text: foto
      - control:
          kind: reset_colour
      - text: " die\nprinses Zelda honderd jaar geleden heeft\ngemaakt."
  CarryingBlueFireEXMini06:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 24
      - text: "Misschien als je de locaties op de "
      - control:
          kind: set_colour
          colour: red
      - text: "foto's"
      - control:
          kind: reset_colour
      - text: "\nbezoekt, dat je je dan iets zult\nherinneren."
  ChottoMatte00:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 4
      - text: "Prinses Zelda vecht in haar eentje tegen\n"
      - control:
