          unknown: 6
  FirstTalk05:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Goed...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Omdat je je geheugen kwijt bent,"
      - control:
          kind: pause
          length: short
      - text: " zal ik je\nalles vertellen wat er is gebeurd."
  FirstTalk06:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Goed dan...
      - control:
          kind: pause
          length: longer
      - text: " Omdat je je geheugen kwijt\nbent,"
      - control:
          kind: pause
          length: short
      - text: " zal ik dit zo vaak je wilt herhalen..."
  FirstTalk07:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "En toen, honderd jaar geleden..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Om ons voor te bereiden op de terugkeer\nvan Ganon, deden we hetzelfde als onze\nvoorouders 10.000 jaar geleden...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Maar... we hadden zijn kracht zwaar\nonderschat..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dus om ervoor te zorgen dat we die fout\nnooit meer maken, heeft de prinses deze\nboodschap voor je achtergelaten..."
  FirstTalk08:
    attributes: Npc_Kakariko001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "En toen, 100 jaar geleden..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Om ons voor te bereiden op de terugkeer\nvan "
      - control:
          kind: set_colour
          colour: red
      - text: Ganon
      - control:
          kind: reset_colour
      - text: ", deden we wat onze\nvoorouders 10.000 jaar geleden deden...\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Maar... We hadden zijn kracht zwaar\nonderschat..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Die fout mogen we nooit meer maken. De\n"
      - control:
          kind: set_colour
