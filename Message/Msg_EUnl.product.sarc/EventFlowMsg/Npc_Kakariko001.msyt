entries:
  "0000":
    attributes: ""
    contents:
      - text: Ik ben er klaar voor.
  "0002":
    attributes: ""
    contents:
      - text: Ik weet het nog niet...
  "0005":
    attributes: ""
    contents:
      - text: Wat is Hateno?
  "0006":
    attributes: ""
    contents:
      - text: Oké.
  "0007":
    attributes: ""
    contents:
      - text: Vier Titanen?
  "0008":
    attributes: ""
    contents:
      - text: Over de Titanen.
  "0009":
    attributes: ""
    contents:
      - text: Geheugen herstellen.
  "0010":
    attributes: ""
    contents:
      - text: Ik ga.
  "0011":
    attributes: ""
    contents:
      - text: Waar moet ik heen?
  "0013":
    attributes: ""
