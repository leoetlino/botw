  Npc_Assassin_0:
    attributes: NPC_PublicVoice
    contents:
      - text: "He he he, jij kijkt niet zo vrolijk..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik verwachtte al dat deze steen veel zou\nbetekenen voor de mensen van dit dorp."
  Npc_Assassin_1:
    attributes: NPC_PublicVoice
    contents:
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: Je hoeft niet zo te schreeuwen.
      - control:
          kind: pause
          length: short
      - text: "\nIk ben er al."
  Npc_Assassin_2:
    attributes: NPC_PublicVoice
    contents:
      - text: Het verbaast me dat jij dat moet vragen.
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dorian, was jij niet de eerste die zei dat\nhij de organisatie wilde verlaten? Dan\nmoet je ook weten wat de gevolgen zijn."
  Npc_Assassin_3:
    attributes: NPC_PublicVoice
    contents:
      - text: Maar...
      - control:
          kind: pause
          length: longer
      - text: " uiteindelijk heb je me toch goed\ngeholpen."
  Npc_Assassin_4:
    attributes: NPC_PublicVoice
    contents:
      - text: Om jou bekommer ik me later wel.
      - control:
          kind: pause
          length: longer
      - text: "\nMaar..."
      - control:
          kind: pause
          length: longer
  Npc_Assassin_5:
    attributes: NPC_PublicVoice
    contents:
      - text: "Ik versla eerst de zwaardvechter die zich\ndaar heeft verstopt!"
  Npc_Assassin_6:
    attributes: NPC_PublicVoice
    contents:
      - text: "Die zwaardvechter daar zal boeten\nvoor wat hij mijn meester heeft\naangedaan..."
  Npc_Kakariko002_Near:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound2
          unknown:
            - 35
            - 205
      - text: Dit is afschuwelijk...
