  Npc_Kakariko002_R_003:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: Kop op?
      - control:
          kind: pause
          length: short
      - text: " Jij hebt makkelijk praten!"
      - control:
          kind: raw
