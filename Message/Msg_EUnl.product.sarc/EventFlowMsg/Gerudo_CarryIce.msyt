entries:
  "0007":
    attributes: ""
    contents:
      - text: Daar ga ik!
  "0008":
    attributes: ""
    contents:
      - text: Nog een keertje...
  "0014":
    attributes: ""
    contents:
      - text: Meiden en jongens.
  "0016":
    attributes: ""
    contents:
      - text: Tot ziens.
  "0017":
    attributes: ""
    contents:
      - text: Maar...
  "0020":
    attributes: ""
    contents:
      - text: IJs?
  "0021":
    attributes: ""
    contents:
      - text: Tot ziens.
  "0022":
    attributes: ""
    contents:
      - text: Zal ik ijs gaan halen?
  Npc_GerudoDesert002_F_001:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 2
      - text: "Ik trek het niet meer... Dit is waar ik zal\nsterven..."
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoDesert002_F_002:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Huh?
  Npc_GerudoDesert002_F_003:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 20
      - text: "De ultieme '"
      - control:
          kind: set_colour
          colour: blue
      - text: meiden en jongens
      - control:
          kind: reset_colour
      - text: "'? Dat\nmoet ik proberen! Ik ga meteen!"
  Npc_GerudoDesert002_S02_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Wees voorzichtig!
  Npc_GerudoDesert003_F_001:
    attributes: Npc_GerudoDesert003
    contents:
      - text: "Heb je het ijs naar Furosa gebracht, dan?\nHm, ik zou zelf ook wel een\n'"
      - control:
          kind: set_colour
          colour: blue
      - text: meiden en jongens
      - control:
          kind: reset_colour
      - text: "' lusten. "
  Npc_GerudoDesert003_R_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dit is een "
      - control:
          kind: set_colour
          colour: blue
      - text: ijshuis
      - control:
          kind: reset_colour
      - text: " waar we ijs opslaan. Dat\nhalen we uit de bergen in het noorden."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het is vrij kostbaar voor ons, dus ik houd\ndeze plek dag en nacht in de gaten."
  Npc_GerudoDesert003_S01_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Oké... Als Furosa dat zegt, dan geloof ik\ndat. Je mag ijs meenemen."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Je hebt waarschijnlijk wel door dat het\nheet is, dus als je er te lang over doet, is\nhet ijs gesmolten voor je terug bent.\nZorg dat je het zo snel mogelijk naar\nFurosa brengt."
  Npc_GerudoDesert003_S01_002:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dit is een "
      - control:
          kind: set_colour
          colour: blue
      - text: ijshuis
      - control:
          kind: reset_colour
      - text: " waar we ijs opslaan. Dat\nhalen we uit de bergen in het noorden."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het is vrij kostbaar, zoals je misschien\nwel zult snappen, dus ik zorg dat het niet\nwordt gestolen."
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoDesert003_S01_101:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: animation
          name: Think_01
      - control:
          kind: text_size
          percent: 80
      - text: "Wie ben jij? Ik ben... vandaag... klaar... met\nwerken... Als je iets wilt... kom je morgen maar\nterug..."
      - control:
          kind: text_size
          percent: 100
  Npc_GerudoDesert003_S01_102:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ik ben klaar met werken voor vandaag.
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoDesert003_S01_103:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 18
      - text: "Het spijt me, maar mijn werk zit erop\nvoor vandaag..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik wil je graag helpen, maar regels zijn\nregels."
  Npc_GerudoDesert003_S01_104:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 18
      - text: "Je hebt pech... Ik ben doodmoe na mijn\nwerk. Ik ga slapen. Als je iets nodig hebt,\nmoet je morgen maar terugkomen."
  Npc_GerudoDesert003_S02_001:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Serieus?
  Npc_GerudoDesert003_S02_002:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dat vermoedde ik al... Bij ons gaat het\nook weleens fout als we ijs vervoeren."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Geen zorgen... Pak gewoon een nieuw\nblok en probeer het nog eens."
  Npc_GerudoDesert003_S02_101:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Je weet hoe heet het in de woestijn kan\nworden. Als je er te lang over doet,\nsmelt het ijs."
      - control:
          kind: choice
          choice_labels:
            - 7
            - 8
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_GerudoDesert003_S02_102:
    attributes: Npc_GerudoDesert003
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ongelofelijk... Ik snap dat dit een verzoek\nvan Furosa is, maar vergeet niet dat ijs\nvoor ons heel kostbaar is!"
  Npc_GerudoMilkflost_002:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Vasaaq...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ben jij een Hyliaanse? Ik weet niet hoe\noud je bent, maar deze plek is niet voor\nkleine meisjes.\nHet ijs dat we hier gebruiken is namelijk\nheel bijzonder... Kom maar terug als je\noud genoeg bent om dat te snappen."
      - control:
          kind: choice
          choice_labels:
            - 14
            - 16
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_GerudoMilkflost_004:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Wil je ons meest beroemde drankje, de\n'"
      - control:
          kind: set_colour
          colour: blue
      - text: meiden en jongens
      - control:
          kind: reset_colour
      - text: "' proberen?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het spijt me, maar daar heb ik heel veel\nijs voor nodig, en momenteel heb ik dat\nniet op voorraad.\nHet is overigens heel erg sterke drank,\ndus dat zou ik niet aan een meisje als jij\nverkopen, zelfs al had ik ijs."
      - control:
          kind: single_choice
          label: 17
  Npc_GerudoMilkflost_005:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Trouwens... Ik heb Pokki al een tijdje niet\ngezien... Normaal kwam ze bijna elke dag\nwel een '"
      - control:
          kind: set_colour
          colour: blue
      - text: meiden en jongens
      - control:
          kind: reset_colour
      - text: "' drinken...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "O, sorry. Dit is niet jouw probleem, dus ik\nzal je er niet lastig mee vallen."
  Npc_GerudoMilkflost_006:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 13
      - text: "Ik zat me net te bedenken dat ik Pokki al\neen tijdje niet heb gezien... Ik maak me\neen beetje zorgen."
  Npc_GerudoMilkflost_007:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Het ijshuis is ten noorden van het dorp.\nAnche staat er altijd op wacht."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als je haar de situatie vertelt, geeft ze je\nvast wel wat ijs mee."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Denk er alleen wel aan dat ze altijd vroeg\nnaar bed gaat."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als je gaat, ga dan liefst zo vroeg\nmogelijk."
  Npc_GerudoMilkflost_008:
    attributes: Npc_GerudoDesert002
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Heb ik net de tempel gevonden...
      - control:
          kind: pause
          length: longer
      - text: " Ugh..."
      - control:
          kind: pause
          length: longer
      - text: "\nDroge strot..."
      - control:
          kind: pause
          length: longer
      - text: " Te zwak..."
      - control:
          kind: pause
          length: longer
      - text: " om te\nbewegen...\nAls dit het einde is..."
      - control:
          kind: pause
          length: longer
      - text: " zou ik graag nog\néén keer een slokje willen van een\nijskoude '"
      - control:
          kind: set_colour
          colour: red
      - text: meiden en jongens
      - control:
          kind: reset_colour
      - text: "'..."
  Npc_GerudoMilkflost_016:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Dankzij jou heb ik mijn beste '"
      - control:
          kind: set_colour
          colour: blue
      - text: "meiden en\njongens"
      - control:
          kind: reset_colour
      - text: "' ooit kunnen maken!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Het spijt me, maar zou jij het aan "
      - control:
          kind: set_colour
          colour: blue
      - text: Pokki
      - control:
          kind: reset_colour
      - text: "\nkunnen vertellen? Zeg haar maar dat ik\ner eentje voor haar heb klaarstaan...\nHaar kennende komt ze dan gelijk\naangerend. Ze is wat... fanatiek als het\nom drank gaat."
  Npc_GerudoMilkflost_017:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Wat zei je? Is dat wat er is gebeurd met\nPokki?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ik bedoel, ik wil haar graag helpen, maar\nik kan niets zonder ijs..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Ik zou ijs kunnen gaan halen bij het\n"
      - control:
          kind: set_colour
          colour: red
      - text: ijshuis
      - control:
          kind: reset_colour
      - text: ", maar mijn knieën zijn niet al te\nbest meer..."
      - control:
          kind: single_choice
          label: 22
  Npc_GerudoMilkflost_018:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Ik vroeg me al af waarom ik haar al een\ntijdje niet had gezien, maar ik wist niet\ndat Pokki in moeilijkheden verkeerde.\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: Waarom zit ik uitgerekend nu zonder ijs?
      - control:
          kind: choice
          choice_labels:
            - 20
            - 21
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_GerudoMilkflost_019:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: "Meen je dat? Dat zou enorm helpen.\nPokki is mijn beste klant, dus ik wil haar\ngraag helpen.\nHet "
      - control:
          kind: set_colour
          colour: red
      - text: ijshuis
      - control:
          kind: reset_colour
      - text: " is ten noorden van het dorp.\nAls je de situatie aan Anche uitlegt, mag\nje vast wel wat ijs meenemen.\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Maar ze gaat altijd vroeg naar bed... dus\nzorg dat je er zo vroeg mogelijk bij bent."
  Npc_GerudoMilkflost_020:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "O,"
      - control:
          kind: pause
          length: long
      - text: " jongedame..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dat ijs het hele eind door de woestijn en\nnaar de stad slepen? Dat lijkt me wat\nveel van het goede.\nEr is een ruïne tussen het fort en het\nijshuis, dus breng het ijs naar de "
      - control:
          kind: set_colour
          colour: red
      - text: "ingang\nvan die ruïne"
      - control:
          kind: reset_colour
      - text: "."
  Npc_oasis025_S02_001:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Kun je het ijs hierheen brengen?
  Npc_oasis025_S02_002:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound2
          unknown:
            - 20
            - 205
      - text: "Héééé! "
      - control:
          kind: pause
          length: long
      - text: Hierheen!
      - control:
          kind: pause
          length: short
      - text: " Breng het ijs hierheen!"
  Npc_oasis025_S02_101:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: "Wauw... Geen twijfel mogelijk. Dit "
      - control:
          kind: set_colour
          colour: red
      - text: ijs
      - control:
          kind: reset_colour
      - text: "\nkomt uit het"
      - control:
          kind: set_colour
          colour: red
      - text: " ijshuis"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Hier kan ik de ultieme '"
      - control:
          kind: set_colour
          colour: blue
      - text: "meiden en\njongens"
      - control:
          kind: reset_colour
      - text: "' mee maken."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Kun jij "
      - control:
          kind: set_colour
          colour: blue
      - text: "Pokki "
      - control:
          kind: reset_colour
      - text: "op de hoogte brengen? Zeg\nhaar maar dat er een heerlijk drankje op\nhaar staat te wachten...\n"
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: "Haar kennende komt ze dan gelijk\naangerend. Ze is wat... fanatiek als het\nom drank gaat."
  Npc_oasis025_S02_104:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 25
      - text: Het ijs ziet er goed uit. Sarqsarq!
      - control:
          kind: pause
          length: long
      - text: "\nHiermee kan ik de perfecte '"
      - control:
          kind: set_colour
          colour: blue
      - text: "meiden en\njongens"
      - control:
          kind: reset_colour
      - text: "' maken.\nKun jij "
      - control:
          kind: set_colour
          colour: blue
      - text: "Pokki "
      - control:
          kind: reset_colour
      - text: "op de hoogte brengen? Zeg\nhaar maar dat er een heerlijk drankje op\nhaar staat te wachten...\n"
      - control:
          kind: sound2
          unknown:
            - 0
            - 205
      - text: "Haar kennende komt ze dan gelijk\naangerend. Ze is wat... fanatiek als het\nom drank gaat."
  Npc_oasis025_S02_105:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: Hm?
      - control:
          kind: pause
          length: long
      - text: " Jij..."
      - control:
          kind: pause
          length: long
      - text: " Nou ja, het maakt ook niet uit\nwie je bent, als ik mijn ijs maar krijg."
  Npc_oasis025_S02_201:
    attributes: Npc_OasisMilk_E
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ik heb nog steeds geen ijs...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Als je het Anche vraagt, geeft ze je vast\nwel ijs. Kun jij het voor me gaan halen?"
  near00:
    attributes: Npc_GerudoDesert002
    contents:
      - text: Ik ben er geweest...
  near01:
    attributes: Npc_OasisMilk_E
    contents:
      - text: Hierheen...
