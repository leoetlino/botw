      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: Dat heb je snel door!
      - control:
          kind: pause
          length: short
      - text: " Ik kan wel zien\nwaarom jij de Uitverkorene bent waar\n"
      - control:
          kind: set_colour
          colour: blue
      - text: prinses Mipha
      - control:
          kind: reset_colour
      - text: " verliefd op werd."
  Npc_Zora004_SD_201:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Het "
      - control:
          kind: set_colour
          colour: red
      - text: lichtpad
      - control:
          kind: reset_colour
      - text: " was de reflectie van de zon\nop de zee?\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Ik kan wel zien waarom jij de\nUitverkorene bent waar "
      - control:
          kind: set_colour
          colour: blue
      - text: prinses Mipha
      - control:
          kind: reset_colour
      - text: "\nverliefd op werd. Erg indrukwekkend!"
  Npc_Zora004_SD_300:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: "Hm, Link!"
      - control:
          kind: pause
          frames: 20
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - text: "Jij hebt geen vinnen of kieuwen, en\nbovendien zink je als een baksteen...\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Maar toch is het je gelukt om dezelfde\nproef te voltooien als"
      - control:
          kind: set_colour
          colour: blue
      - text: " Mipha"
      - control:
          kind: reset_colour
      - text: "!\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Het verbaast me niets dat "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " een\nspeciaal plekje voor je heeft in haar hart.\nIk ben diep onder de indruk."
  Npc_Zora004_SD_350:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 10
      - text: "Het is je gelukt om dezelfde proef te\nvoltooien als"
      - control:
          kind: set_colour
          colour: blue
      - text: " Mipha"
      - control:
          kind: reset_colour
      - text: "!\n\nHet verbaast me niets dat "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " een\nspeciaal plekje voor je heeft in haar hart.\nIk ben diep onder de indruk."
  Npc_Zora014_SD_000:
    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wat denk je ervan? Kun je me mijn\nbotte gedrag vergeven, zoals sporen\ndie door water worden weggespoeld?\n..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: "Vergeet alsjeblieft ook wat ik zojuist\nheb gezegd..."
      - control:
          kind: pause
          length: longer
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
