    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: In de teksten van
      - control:
          kind: set_colour
          colour: blue
      - text: " Mipha"
      - control:
          kind: reset_colour
      - text: " staat het\nvolgende: \"De "
      - control:
          kind: set_colour
          colour: red
      - text: ochtendzon
      - control:
          kind: reset_colour
      - text: " is je troef, om\nhet "
      - control:
          kind: set_colour
          colour: red
      - text: "lichtpad "
      - control:
          kind: reset_colour
      - text: "te vinden naar de proef.\"\nDeze woorden hebben iets met de proef\nte maken... De prinses herhaalde ze\ndiverse keren."
      - control:
          kind: pause
          length: long
  Npc_Zora004_SD_111:
    attributes: Npc_Zora004
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "O, jij bent het, Link!\n\n\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " heeft teksten over de proef\ngevonden, en daar staat het volgende in:\n\n\"De "
      - control:
          kind: set_colour
          colour: red
      - text: ochtendzon
      - control:
          kind: reset_colour
      - text: " is je troef, om het\n"
      - control:
          kind: set_colour
          colour: red
      - text: "lichtpad "
      - control:
          kind: reset_colour
      - text: "te vinden naar de proef.\"\n\nIk vraag me af wat dat betekent..."
