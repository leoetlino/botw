            - 11
            - 16
      - text: "Destijds heeft "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
      - text: " met ze\nafgerekend... Maar wie zou dat nu\nkunnen doen?"
  Npc_Zora014_SD_100:
    attributes: Npc_Zora014
