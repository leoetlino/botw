    attributes: Npc_Zora014
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De wezens die opdoken toen "
      - control:
          kind: set_colour
          colour: blue
      - text: Mipha
      - control:
          kind: reset_colour
