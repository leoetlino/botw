          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Musician_AoC_One_Near_01:
    attributes: Npc_Musician_AoC_BalladOfHeroes
    contents:
      - text: Een oorverdovend geluid...
  Npc_Musician_AoC_One_Near_02:
    attributes: Npc_Musician_AoC_BalladOfHeroes
    contents:
      - text: Wat een somber weer...
  Obj_BlowSword_010:
    attributes: NPC_GodVoice
    contents:
      - text: Je hebt geen plek meer voor wapens...
