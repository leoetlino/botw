  Npc_Musician_AoC_HeroXxxx_Demo610_010:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Zag ik nou licht? Of was dat mijn\nverbeelding..."
      - control:
          kind: pause
          length: long
