    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Gaf het "
      - control:
          kind: set_colour
          colour: blue
      - text: stenen monument
      - control:
          kind: reset_colour
      - text: " nou licht?\nIk denk niet dat ik het me verbeeldde..."
  Npc_Musician_AoC_HeroXxxx_Demo610_030:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: "Dit "
      - control:
          kind: set_colour
          colour: blue
      - text: stenen monument
      - control:
          kind: reset_colour
      - text: " is al tienduizend\njaar bewaard gebleven..."
      - control:
          kind: pause
          length: long
      - text: " Wat zou het\nvoorstellen?"
      - control:
          kind: pause
          length: long
  Npc_Musician_AoC_HeroXxxx_Demo610_040:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: Dat licht...
      - control:
          kind: pause
          length: long
      - text: "\n\n\nReageert het soms op bezoekers?\nReageert het op mij of..."
      - control:
          kind: pause
          length: long
  Npc_Musician_AoC_HeroXxxx_Demo610_First_010:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 8
      - text: Hm?
      - control:
          kind: pause
          length: longer
      - text: " Dat ding op je heup."
      - control:
          kind: pause
          length: long
      - text: "\n\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "O, dat gaat me eigenlijk ook niets aan."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_020:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hoe dan ook, we zijn allebei reizigers...\nWaarom maken we niet even een praatje?"
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Npc_Musician_AoC_HeroXxxx_Demo610_First_030:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 0
      - text: "Als ik je zie op een locatie die verband\nhoudt met een "
      - control:
          kind: set_colour
          colour: blue
      - text: oud lied
      - control:
          kind: reset_colour
      - text: ", zou ik graag\nvoor je zingen.\nO, ik was het bijna vergeten."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_050:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Ja, het lied is bijna compleet, maar..."
  Npc_Musician_AoC_HeroXxxx_Demo610_First_060:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 1
      - text: "Toen mijn meester deze wereld verliet,\nhad het lied een paar "
      - control:
          kind: set_colour
          colour: blue
      - text: ontbrekende delen
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: "Ik wilde zijn werk voltooien.\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "En daarom bezoek ik de gebieden die in\nhet lied genoemd worden."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroBallad_001:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wat vind je ervan? Dat was het "
      - control:
          kind: set_colour
          colour: blue
      - text: "vers\nover de Uitverkorene"
      - control:
          kind: reset_colour
      - text: " en hoe deze de\nproef onderging."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroBallad_002:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wat vind je ervan?\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Door in elke regio naar de "
      - control:
          kind: set_colour
          colour: blue
      - text: Uitverkorene
      - control:
          kind: reset_colour
      - text: "\nte vragen, kom ik aan genoeg materiaal\nvoor de"
      - control:
          kind: set_colour
          colour: blue
      - text: " ontbrekende delen"
      - control:
          kind: reset_colour
      - text: " van het lied."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroBallad_003:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wat vind je ervan?\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hoe vaker ik naar hun legendes luister,\nhoe beter ik de "
      - control:
          kind: set_colour
          colour: blue
      - text: Uitverkorenen
      - control:
          kind: reset_colour
      - text: " begrijp.\n\nEn dat begrip helpt me de "
      - control:
          kind: set_colour
          colour: blue
      - text: "ontbrekende\ndelen"
      - control:
          kind: reset_colour
      - text: " van het lied te schrijven."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroBallad_004:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Wat vind je ervan?\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Omdat ik de "
      - control:
          kind: set_colour
          colour: blue
      - text: Uitverkorenen
      - control:
          kind: reset_colour
      - text: " begrijp,\nlukt het me om de "
      - control:
          kind: set_colour
          colour: blue
      - text: ontbrekende delen
      - control:
          kind: reset_colour
      - text: "\nvan het lied te schrijven."
  Npc_Musician_AoC_HeroXxxx_Demo610_HeroFirst_011:
    attributes: Npc_Musician_AoC_HeroGoronRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: Hoe dan ook...
  Npc_Musician_AoC_HeroXxxx_Demo610_Relief_001:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Ah, dit "
      - control:
          kind: set_colour
          colour: blue
      - text: monument
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: "\nIk kan mijn ogen niet geloven.\n\nHet "
      - control:
          kind: set_colour
          colour: blue
      - text: monument
      - control:
          kind: reset_colour
      - text: " dat wordt genoemd in\nhet "
      - control:
          kind: set_colour
          colour: blue
      - text: onvoltooide lied
      - control:
          kind: reset_colour
      - text: " van mijn meester...\nHet verscheen vlak voor me..."
      - control:
          kind: single_choice
          label: 7
  Npc_Musician_AoC_HeroXxxx_Demo610_Relief_010:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Ah, dit "
      - control:
          kind: set_colour
          colour: blue
      - text: monument
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " Eerlijk gezegd\nverraste het me wel!\n\nHet "
      - control:
          kind: set_colour
          colour: blue
      - text: monument
      - control:
          kind: reset_colour
      - text: " dat wordt genoemd in\nhet "
      - control:
          kind: set_colour
          colour: blue
      - text: onvoltooide lied
      - control:
          kind: reset_colour
      - text: " van mijn meester...\nHet verscheen vlak voor me..."
      - control:
          kind: single_choice
          label: 7
  Npc_Musician_AoC_HeroXxxx_Demo610_Relief_011:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 11
      - text: "Kijk hier eens! Een van de "
      - control:
          kind: set_colour
          colour: blue
      - text: monumenten
      - control:
          kind: reset_colour
      - text: "\ndie in het "
      - control:
          kind: set_colour
          colour: blue
      - text: "onvoltooide lied "
      - control:
          kind: reset_colour
      - text: "van mijn\nmeester genoemd worden!"
  Npc_Musician_AoC_HeroXxxx_Demo610_Relief_012:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 23
      - text: "Kijk! Net als ik dacht! Een van de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: monumenten
      - control:
          kind: reset_colour
      - text: " uit het "
      - control:
          kind: set_colour
          colour: blue
      - text: onvoltooide lied
      - control:
          kind: reset_colour
      - text: "\nvan mijn meester!"
  Npc_Musician_AoC_HeroXxxx_Demo610_Relief_013:
    attributes: Npc_Musician_AoC_HeroZoraRelief
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: "Kijk! Een van de monumenten uit het\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "onvoltooide lied "
      - control:
          kind: reset_colour
      - text: "van mijn meester!\nZoals je al wel geraden hebt."
  Npc_Musician_AoC_HeroXxxx_Demo610_Town_001:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo's"
      - control:
          kind: reset_colour
      - text: " zijn een aardig volkje.\n\n\nDe "
      - control:
          kind: set_colour
          colour: blue
      - text: "matriarch "
      - control:
          kind: reset_colour
      - text: "en de "
      - control:
          kind: set_colour
          colour: blue
      - text: soldaten
      - control:
          kind: reset_colour
      - text: " konden me\nveel vertellen over de Uitverkorene. Ik\nheb interessante verhalen gehoord."
  Npc_Musician_AoC_HeroXxxx_Demo610_Town_002:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: Gorons
      - control:
          kind: reset_colour
      - text: " zijn erg praatgraag.\n\n\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Yunobo
      - control:
          kind: reset_colour
      - text: " en "
      - control:
          kind: set_colour
          colour: blue
      - text: Bludo
      - control:
          kind: reset_colour
      - text: " hebben me een paar\ninteressante verhalen verteld over de\nUitverkorene."
  Npc_Musician_AoC_HeroXxxx_Demo610_Town_003:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "De "
      - control:
          kind: set_colour
          colour: blue
      - text: koning
      - control:
          kind: reset_colour
      - text: " en "
      - control:
          kind: set_colour
          colour: blue
      - text: prins
      - control:
          kind: reset_colour
      - text: " van het "
      - control:
          kind: set_colour
          colour: blue
      - text: Zora-paleis
      - control:
          kind: reset_colour
      - text: "\nwaren heel vriendelijk.\n\nZe hebben allerlei verhalen over de\nUitverkorene met me gedeeld."
  Npc_Musician_AoC_HeroXxxx_Demo610_Town_004:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik ben teruggegaan naar het "
      - control:
          kind: set_colour
          colour: blue
      - text: Rito-dorp
      - control:
          kind: reset_colour
      - text: "\nom weer even thuis te zijn...\n\nIk had het geluk dat "
      - control:
          kind: set_colour
          colour: blue
      - text: Teba
      - control:
          kind: reset_colour
      - text: " en "
      - control:
          kind: set_colour
          colour: blue
      - text: Kaneli
      - control:
          kind: reset_colour
      - text: "\nbereid waren om me verhalen over de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Uitverkorene
      - control:
          kind: reset_colour
      - text: " te vertellen."
  Npc_Musician_AoC_HeroXxxx_Demo612_010:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - control:
          kind: animation
          name: QuestionNormal_01
      - text: "O? Nou, nou...\n\n\nHet beste aan alleen reizen is dat je\nverdwaald kunt raken in je eigen\ngedachten, zonder te worden afgeleid."
  Npc_Musician_AoC_HeroXxxx_Demo612_020:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "Misschien is het geen toeval dat ik je\nhier ontmoet.\n\nDit is voor jou de perfecte plek om in je\neigen gedachten te verdwalen en voor\nmij om liederen te schrijven."
  Npc_Musician_AoC_HeroXxxx_Demo612_030:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: "We hebben verschillende doelen, maar\nwe bewandelen hetzelfde pad...\n\nNou ja, ik vlieg. Ik wandel niet..."
  Npc_Musician_AoC_HeroXxxx_Demo612_040:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 6
            - 24
      - text: "Ben je klaar met verdwalen in je eigen\ngedachten?\n\nJe leek diep in gedachten verzonken.\nSorry, dat gaat me eigenlijk niks aan, h√®?"
  Npc_Musician_AoC_HeroXxxx_Demo612_Grow_001:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: animation
          name: Understand_01
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Ik had sterk het gevoel dat het\nontbrekende deel van het lied door mij\nzou moeten worden geschreven."
  Npc_Musician_AoC_HeroXxxx_Demo612_Grow_002:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - control:
          kind: animation
          name: Understand_01
      - text: "Ik heb er nu veel meer vertrouwen in dat\nik de ontbrekende delen van het lied\nkan schrijven."
  Npc_Musician_AoC_HeroXxxx_Demo612_Grow_003:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - control:
          kind: animation
          name: Understand_00
      - text: "Het eindresultaat is nog ongewis, maar\nik weet wel al waar de ontbrekende\ncoupletten over moeten gaan."
  Npc_Musician_AoC_HeroXxxx_Demo612_Grow_004:
    attributes: Npc_Musician_AoC_HeroZora
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - control:
          kind: animation
          name: TalkPositive_01
      - text: "Het "
      - control:
          kind: set_colour
          colour: blue
      - text: ontbrekende deel
      - control:
          kind: reset_colour
      - text: " van het lied...\nHet vroegere "
      - control:
          kind: set_colour
          colour: blue
      - text: Kasteel Hyrule
      - control:
          kind: reset_colour
      - text: " lijkt me de\nperfecte setting."
  Npc_Musician_AoC_Individual_020:
    attributes: Npc_Musician_005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "En?\nWil je het "
      - control:
          kind: set_colour
          colour: blue
      - text: lied
      - control:
          kind: reset_colour
      - text: " horen waarin de\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Uitverkorene
      - control:
          kind: reset_colour
      - text: " wordt geprezen?"
      - control:
          kind: choice
