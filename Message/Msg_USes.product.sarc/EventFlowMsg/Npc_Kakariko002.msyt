      - control:
          kind: choice
          choice_labels:
            - 8
            - 11
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk34:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Estaba sacando brillo a las estatuillas\nde las deidades protectoras."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Son símbolos que nos guían a lo largo de\nnuestras vidas."
      - control:
          kind: pause
          length: short
