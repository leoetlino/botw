    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 37
      - text: Qué vergüenza...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Dime, ¿puedo ayudarte en algo más?"
      - control:
