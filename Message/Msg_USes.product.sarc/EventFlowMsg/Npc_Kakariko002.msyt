    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Debo ocuparme de la reliquia.
  Talk32:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: ¿Qué otra cosa necesitas?
      - control:
