    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: No a todo el mundo le gusta la lluvia...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 1
            - 11
