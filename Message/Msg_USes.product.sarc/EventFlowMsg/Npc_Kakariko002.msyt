            - 9
            - 10
            - 11
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk33:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: "¡Pero si eres tú, Link! ¿Cómo se te\nocurre darme semejante susto?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Y bien, ¿qué ocurre?"
      - control:
          kind: pause
