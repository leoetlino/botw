      - control:
          kind: pause
          length: short
      - text: "\nI really wish he'd grow up a little..."
  Goron_Father_Talk20:
    attributes: Npc_Goron010
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Southwest of "
      - control:
