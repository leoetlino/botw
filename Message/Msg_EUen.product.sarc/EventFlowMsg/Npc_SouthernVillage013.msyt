      - control:
          kind: sound
          unknown:
            - 6
            - 0
      - text: Are you a traveler?
      - control:
          kind: pause
          length: long
      - text: "\nWelcome to Lurelin Village.\n\nI'm Rozel."
      - control:
          kind: pause
          length: long
      - text: " I'm the head of this village."
