    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "The final location of Princess Zelda's\nlost memories...\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 17
      - text: "Even I do not know where it is.\n\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
