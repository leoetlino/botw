          colour: red
      - text: Calamity Ganon
      - control:
          kind: reset_colour
      - text: ",\nwhile he is weak!"
      - control:
          kind: pause
          length: short
      - text: " Hurry to the princess!\nGo, now!"
  Talk_09:
    attributes: Npc_Kakariko001
    contents:
