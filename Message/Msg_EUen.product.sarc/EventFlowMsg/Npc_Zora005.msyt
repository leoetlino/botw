          unknown: 6
  talk21:
    attributes: Npc_Zora005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: I thought so.
      - control:
          kind: pause
          length: long
      - text: " Thanks to the benevolence\nof the Goddess Hylia,"
      - control:
          kind: pause
          length: short
      - text: " I can offer you a\nplace to stay.\nWhen would you like me to wake you?"
      - control:
