  Talk13:
    attributes: Npc_HatenoGate003
    contents:
      - text: "I wasn't sleeping..."
      - control:
          kind: pause
          length: longer
      - text: " I wasn't!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "I was praying for the souls of the warrior\nand all who died here at "
      - control:
          kind: set_colour
          colour: blue
      - text: Fort Hateno
      - control:
          kind: reset_colour
      - text: "."
      - control:
