          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: longer
      - text: " "
      - control:
          kind: reset_colour
      - text: "What happened\nto the plum trees?!\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "This plum garden is very special to my\nwife."
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: reset_colour
      - text: "She's going to be awfully sad...\n\n"
      - control:
