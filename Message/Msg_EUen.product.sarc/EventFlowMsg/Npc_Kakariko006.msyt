      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - text: " "
      - control:
          kind: reset_colour
      - text: "Please be considerate\nof that."
  Talk42:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 20
            - 205
      - text: WHAT ARE YOU DOING?!
