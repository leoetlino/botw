    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "What are you doing out in the middle\nof the night?"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n\nAs for me, the first step to achieving\ngreat defense is training your legs.\nThat's why I'm running at this hour.\nTraining is the best way to learn the\nsecret art of defense, after all."
      - control:
          kind: choice
          choice_labels:
            - 4
