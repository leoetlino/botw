          length: long
  Talk22:
    attributes: Npc_Musician_006
    contents:
      - control:
          kind: auto_advance
          frames: 250
      - control:
          kind: raw
          one:
            one:
              field_1: 4
              field_2: 1050253722
      - text: "The hero, the princess—"
      - control:
          kind: pause
          length: long
      - text: hand in hand—
      - control:
          kind: pause
          length: long
      - text: "\nMust bring the light"
      - control:
          kind: pause
          length: long
      - text: " back to this land."
      - control:
          kind: pause
          length: long
  Talk23:
    attributes: Npc_Musician_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Thank you.
      - control:
          kind: pause
          length: short
      - text: " Well then..."
  Talk24:
    attributes: Npc_Musician_006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Of course.
      - control:
