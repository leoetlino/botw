            - 1
            - 4
      - text: "Ich bin gerettet! "
      - control:
          kind: pause
          length: short
      - text: "Das war ganz schön\ngefährlich!"
  SavedLv1_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 2
      - text: Hey!
      - control:
          kind: pause
