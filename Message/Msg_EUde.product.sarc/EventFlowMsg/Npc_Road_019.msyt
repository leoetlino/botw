            - 0
      - text: "Du bist stärker, als du aussiehst."
  SavedLv2_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 0
      - text: Dank dir bin ich in Sicherheit!
      - control:
          kind: pause
