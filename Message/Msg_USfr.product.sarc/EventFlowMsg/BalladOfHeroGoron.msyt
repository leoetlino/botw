    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Hé! "
      - control:
          kind: pause
          length: long
      - text: "Mais t'es le p'tit bonhomme que j'ai\nrencontré sur le site du grand "
      - control:
          kind: set_colour
          colour: blue
      - text: Daruk
      - control:
          kind: reset_colour
      - text: "!\n\nTu sais,"
      - control:
          kind: pause
          length: short
      - text: " depuis mon pèlerinage, j'arrive\npas à me sortir notre Prodige de la tête.\nQuelle bravoure, tout de même!"
