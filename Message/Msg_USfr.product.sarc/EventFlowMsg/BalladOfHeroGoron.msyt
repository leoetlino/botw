      - control:
          kind: reset_colour
      - text: ".\nBah, je te le donnerai une autre fois!"
  Npc_Goron022_Talk13:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Tiens, ça vient du site célèbre où s'est\nrendu le grand "
      - control:
          kind: set_colour
          colour: blue
      - text: Daruk
      - control:
          kind: reset_colour
      - text: ". Ça te fera un\nsouvenir."
  Npc_Goron022_Talk14:
    attributes: Npc_Goron022
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "J'me disais qu'en fait, "
      - control:
          kind: pause
          length: short
      - text: "tu es vraiment\nvenu au bon moment!\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Le drôle de lézard qui se baladait sur\nle "
      - control:
          kind: set_colour
