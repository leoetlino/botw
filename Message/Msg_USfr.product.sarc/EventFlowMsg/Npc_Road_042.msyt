  MSG_Akka_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "T'as du minerai?"
      - control:
          kind: pause
          length: longer
      - text: " Parce que si t'en as,"
      - control:
          kind: pause
          length: short
      - text: "\nvends-le dans une boutique, certains\nvalent une fortune."
  MSG_Akka_00_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Si t'as du minerai et que tu sais pas quoi\nen faire, va le vendre dans une boutique."
      - control:
          kind: pause
          length: longer
      - text: "\nPlus ça brille, plus ça vaut cher!"
  MSG_Akka_01:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "T'es déjà allé au "
      - control:
          kind: set_colour
          colour: blue
      - text: village Goron
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: long
      - text: "\nIl se situe sur le "
      - control:
          kind: set_colour
          colour: blue
      - text: mont de la Mort
      - control:
          kind: reset_colour
      - text: ",\nà l'ouest d'ici.\nPar contre,"
      - control:
          kind: pause
          length: short
      - text: " il paraît que seuls les Gorons\npeuvent l'atteindre,"
      - control:
          kind: pause
          length: short
      - text: " à cause de la\nchaleur atroce."
  MSG_Akka_01_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Le "
      - control:
          kind: set_colour
          colour: blue
      - text: village Goron
      - control:
          kind: reset_colour
      - text: " se trouve sur le\n"
      - control:
          kind: set_colour
          colour: blue
      - text: mont de la Mort
      - control:
          kind: reset_colour
      - text: ","
      - control:
          kind: pause
          length: short
      - text: " à l'ouest d'ici.\n\nLe trajet est pénible, "
      - control:
          kind: pause
          length: short
      - text: "tellement il fait\nchaud,"
      - control:
          kind: pause
          length: short
      - text: " mais y a de super boutiques.\nTu devrais y aller au moins une fois!"
  MSG_Akka_02:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Tu connaîtrais pas un Goron qui\ns'appelle Uldon, par hasard?"
      - control:
          kind: pause
          length: longer
      - text: "\nC'est mon frère!\nC'est un brave p'tit gars, pas du genre\nà se laisser démonter!"
      - control:
          kind: pause
          length: longer
      - text: " En ce moment,\nil voyage je n'sais où..."
  MSG_Akka_02_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Uldon
      - control:
          kind: pause
          length: short
      - text: ", mon frère, "
      - control:
          kind: pause
          length: short
      - text: "est en voyage\nje n'sais pas où...\n\nSi tu le croises, "
      - control:
          kind: pause
          length: short
      - text: "un de ces jours,"
      - control:
          kind: pause
          length: short
      - text: " ce serait\nfin que tu prennes un instant pour\ndiscuter avec lui."
  MSG_Akka_03:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Le minerai,"
      - control:
          kind: pause
          length: short
      - text: " ça vaut une fortune!\n\n\nAlors, si tu en trouves au détour d'un\nchemin,"
      - control:
          kind: pause
          length: short
      - text: " j'te conseille vivement de pas\ncracher d'ssus!"
  MSG_Akka_03_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Quand tu trouves du minerai au détour\nd'un chemin,"
      - control:
          kind: pause
          length: short
      - text: " va l'extraire!\n\nT'obtiendras p'têtre même des gemmes\nde valeur en prime,"
      - control:
          kind: pause
          length: short
      - text: " alors c'est tout\nà ton avantage!"
  MSG_Akka_04:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Bienv'nue au "
      - control:
          kind: set_colour
          colour: blue
      - text: "relais de l'est d'Akkala"
      - control:
          kind: reset_colour
      - text: "!\n\n\nT'sais, le minerai, "
      - control:
          kind: pause
          length: short
      - text: "c'est appétissant,\n"
      - control:
          kind: pause
          length: short
      - text: "mais si t'en as, retiens-toi d'le manger\net revends-le plutôt à c'relais!\nAh, mais j'oubliais qu'y a que les Gorons\nqui apprécient le goût et le croustillant\ndu minerai..."
  MSG_Akka_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Les relais, c'est pratique,"
      - control:
          kind: pause
          length: short
      - text: " ils te rachètent\nles trucs dont tu veux t'débarrasser.\n\nGenre, moi, les cailloux, je les dévore,"
      - control:
          kind: pause
          length: short
      - text: "\nmais pour un "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylien
      - control:
          kind: reset_colour
      - text: " comme toi,"
      - control:
          kind: pause
          length: short
      - text: " vaut\np'têtre mieux les revendre?"
