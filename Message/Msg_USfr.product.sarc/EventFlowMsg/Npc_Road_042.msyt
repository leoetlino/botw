  RetMSG_Akka_00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Nous, les Gorons, on se nourrit de\ncailloux! "
      - control:
          kind: pause
          length: long
      - text: "Mais du coup, la question\nexistentielle de notre vie, c'est...\n... Et le minerai?"
      - control:
          kind: pause
          length: short
      - text: " On en fait quoi?"
      - control:
          kind: pause
          length: short
      - text: " On le\nmange? On le vend?"
      - control:
          kind: pause
          length: short
      - text: " C'est que ça vaut\ncher!"
      - control:
          kind: pause
          length: short
      - text: " Quel dilemme..."
  RetMSG_Akka_00_1:
    attributes: ""
    contents:
      - control:
