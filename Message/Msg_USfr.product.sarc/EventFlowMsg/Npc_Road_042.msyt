      - text: "relais du\nsud d'Akkala"
      - control:
          kind: reset_colour
      - text: "..."
      - control:
          kind: pause
          length: long
      - text: " Je m'rappelle pas\ntoujours lequel est où?\nAttends, ici,"
      - control:
          kind: pause
          length: long
      - text: " on est au relaaaaais... "
      - control:
          kind: pause
          length: long
      - text: du...
      - control:
          kind: pause
          length: long
      - text: "\nBah, à quoi bon, j'me rappelle plus!"
  RetMSG_Akka_04_1:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Le "
      - control:
          kind: set_colour
          colour: blue
      - text: "relais de l'est d'Akkala"
      - control:
          kind: reset_colour
      - text: " et le "
      - control:
          kind: set_colour
