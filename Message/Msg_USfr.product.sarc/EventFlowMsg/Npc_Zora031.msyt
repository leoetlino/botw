            - 11
            - 16
      - text: "Oh... "
      - control:
          kind: pause
          length: short
      - text: "J'y pense..."
      - control:
          kind: pause
          length: short
      - text: " Vous devriez éviter\nde parler aux anciens, maître Link...\n\n"
      - control:
          kind: sound
