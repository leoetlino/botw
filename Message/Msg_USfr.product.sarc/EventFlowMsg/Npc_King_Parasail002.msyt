      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Tiens tiens...
      - control:
          kind: pause
          length: long
      - text: " En voilà une surprise!"
  Appear02:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 13
      - text: "Toutes ces tours qui s'élèvent\nles unes après les autres...\n\nSoit je me trompe fort,"
      - control:
          kind: pause
          length: long
      - text: " soit une grande\npuissance longtemps assoupie refait\nsurface à Hyrule."
  Appear03:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Alors...
      - control:
          kind: pause
          length: long
