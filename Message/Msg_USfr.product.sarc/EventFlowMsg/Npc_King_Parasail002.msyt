  Anataha00:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound2
          unknown:
            - 4
            - 205
      - text: "Ah ça y est, tu t'intéresses à moi,\nmaintenant!\n\nEh bien..."
      - control:
          kind: pause
          length: long
      - text: " Je suis un vieil homme un\npeu fantasque qui vit tout seul ici\ndepuis un moment."
  Appear01:
    attributes: Npc_King_Parasail002
