          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: text_size
          percent: 80
      - text: "\n\n"
      - control:
          kind: sound
          unknown:
            - 2
            - 0
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: text_size
          percent: 125
      - text: Quand!!!
      - control:
          kind: text_size
          percent: 100
      - text: "\n\n\nJ'ai appris qu'un "
      - control:
          kind: set_colour
