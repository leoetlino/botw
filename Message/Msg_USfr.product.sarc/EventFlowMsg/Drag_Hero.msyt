          colour: blue
      - text: "cité\nGerudo"
      - control:
          kind: reset_colour
      - text: " pour faire du commerce...\n\nMais apparemment, seules les "
      - control:
          kind: set_colour
          colour: red
      - text: "femmes\nsont autorisées à entrer"
      - control:
          kind: reset_colour
      - text: ". C'est injuste,\non a fait tout ce chemin pour rien..."
