  Thanks00:
    attributes: ""
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 1
      - text: Je te dois une fière chandelle!
      - control:
          kind: pause
          length: long
      - text: "\nTiens, accepte ça en remerciement!"
