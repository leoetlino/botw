  talk002:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: "Oh, ben!"
      - control:
          kind: pause
          length: longer
      - text: " Ça fait un longtemps que j'ai\npas vu une jolie poupée comme..."
      - control:
          kind: pause
          length: long
      - text: " Heille!\n\n"
      - control:
          kind: sound
          unknown:
            - 3
            - 0
      - text: "T'es pas une poupoune,\nt'es un méchant moineau...\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "T'arrives peut-être à berner les Gerudos\navec ton linge,"
      - control:
          kind: pause
          length: short
      - text: " mais ça marche pas\navec un "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylien
      - control:
          kind: reset_colour
      - text: ".\nDepuis que j'suis ici,"
      - control:
          kind: pause
          length: short
      - text: " j'vois juste passer\ndes gars."
      - control:
          kind: pause
          length: short
      - text: " Pas une seule fille..."
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk005:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hmm? "
      - control:
          kind: pause
          length: short
      - text: "Oh, c'est juste toi..."
      - control:
          kind: pause
          length: longer
      - text: "\nTu m'veux quoi?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
            - 2
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk010:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 3
            - 12
      - text: "Dans un relais crotté, sans une seule\nfille.\n\nY'a ben un Zora qui est passé l'autre\njour..."
      - control:
          kind: pause
          length: short
      - text: " Encore un gars."
      - control:
          kind: pause
          length: longer
      - text: " Même qu'y se\nfaisait appeler « "
      - control:
          kind: set_colour
          colour: red
      - text: prince
      - control:
          kind: reset_colour
      - text: " ».\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Heureusement que Satori est là pour\nme consoler..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 5
            - 4
          selected_index: 0
          cancel_index: 2
          unknown: 8
  talk020:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "R'garde un peu les nuages à l'est."
      - control:
          kind: pause
          length: longer
      - text: "\nC'est bizarre, hein?\n\nJ'aime pas la pluie, ça m'rend triste."
      - control:
          kind: pause
          length: longer
      - text: "\nUne chance que Satori est là pour\nme consoler..."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk030:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Super, des excuses...\n\n\nDepuis que j'suis arrivé ici, j'ai vu\npasser que toi et l'autre hurluberlu, \nle « "
      - control:
          kind: set_colour
          colour: red
      - text: "prince Zora "
      - control:
          kind: reset_colour
      - text: "».\nY'a juste des gars dans ce relais, j'te dis.\nPourtant, y'a pas de panneau qui dit\n« Interdit aux femmes » à l'entrée..."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk040:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: "Des types gluants avec une face de\npoisson."
      - control:
          kind: pause
          length: longer
      - text: " T'en as jamais vu?\n\nY'habitent à l'est d'ici, dans le domaine\ndes Zoras."
      - control:
          kind: pause
          length: short
      - text: " Faut juste remonter la\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "Zora "
      - control:
          kind: reset_colour
      - text: "pour y arriver.\nEt donc, ce type vient vers moi, me traite\nd'« "
      - control:
          kind: set_colour
          colour: blue
      - text: Hylien
      - control:
          kind: reset_colour
      - text: " valide », et veut que j'le suive\npour j'sais plus trop quoi.\n"
      - control:
          kind: sound
          unknown:
            - 5
            - 12
      - text: "J'suis pas un mauvais gars, mais j'avais\npeur qu'il arrive quelque chose à\nSatori."
      - control:
          kind: pause
          length: short
      - text: " Faque, j'ai refusé."
      - control:
          kind: choice
          choice_labels:
            - 5
            - 4
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk050:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "C'est ça,"
      - control:
          kind: pause
          length: short
      - text: " salut."
  talk060:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Ah... "
      - control:
          kind: set_colour
          colour: blue
      - text: Satori
      - control:
          kind: reset_colour
      - text: "... "
      - control:
          kind: pause
          length: short
      - text: "Y m'a sauvé la vie...\nMais c'est une longue histoire, pas le\ngoût de t'achaler avec ça."
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk061:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "C'est toi l'chef.\n\n\nUne "
      - control:
          kind: set_colour
          colour: red
      - text: montagne louche
      - control:
          kind: reset_colour
      - text: " avec des bestiaux\nqu'apparaissent qu'à certaines heures,\nça te dit quelque chose?\nQuand j'ai entendu cette histoire,\nj'ai tout d'suite senti le bon coup.\n\nJ'pensais aller chasser tout ça, histoire\nde me faire quelques rubis faciles..."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk062:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Alors, j'y suis allé."
      - control:
          kind: pause
          length: short
      - text: " J'me suis trouvé\nun coin tranquille, pis j'ai attendu.\n\nEt v'là qu'un chiot tout sale débarque."
      - control:
          kind: pause
          length: longer
      - text: "\nP'têt qu'il avait perdu ses parents, ou\nqu'on l'avait abandonné, j'sais pas trop...\nEn tout cas, y s'est mis à japper."
      - control:
          kind: pause
          length: longer
      - text: "\nY devait crever d'faim, alors j'lui ai\ndonné d'la viande pour qu'il la ferme..."
      - control:
          kind: choice
          choice_labels:
            - 8
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk063:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Pis une nuit, j'me suis endormi..."
      - control:
          kind: pause
          length: long
      - text: " En\nrouvrant les yeux, y'avait un machin, un\n"
      - control:
          kind: set_colour
          colour: blue
      - text: "bestiau "
      - control:
          kind: reset_colour
      - text: "tout brillant planté devant moi.\nÇa m'a tellement pris par surprise que\nj'suis resté là sans chialer!"
      - control:
          kind: pause
          length: long
      - text: " J'ai même\npas pensé à sortir mon arc...\nPendant ce temps, l'"
      - control:
          kind: set_colour
          colour: blue
      - text: "bestiau "
      - control:
          kind: reset_colour
      - text: "avançait vers\nmoi."
      - control:
          kind: pause
          length: short
      - text: " J'tais sûr qu'il allait m'écraser."
      - control:
          kind: pause
          length: short
      - text: " J'me\nvoyais déjà rencontrer mes ancêtres...\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Et qui arrive à ce moment-là?
      - control:
          kind: pause
          length: long
      - text: "\nLe bâtard à qui j'avais donné à bouffer!\n\nEt tout d'suite, y s'met à aboyer sur\nl'"
      - control:
          kind: set_colour
          colour: blue
      - text: "bestiau "
      - control:
          kind: reset_colour
      - text: "qui devait faire dix fois sa taille!\n\nTout ça..."
      - control:
          kind: pause
          length: longer
      - text: " Juste pour me protéger..."
      - control:
          kind: pause
          length: long
      - text: "\nAlors qu'il tremblait de peur..."
      - control:
          kind: choice
          choice_labels:
            - 10
            - 9
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk064:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: "Le "
      - control:
          kind: set_colour
          colour: blue
      - text: bestiau
      - control:
          kind: reset_colour
      - text: " a hésité une seconde...\n\n\nAlors, j'ai pas réfléchi."
      - control:
          kind: pause
          length: long
      - text: " J'ai ramassé le\nchien, pis j'ai pris mes jambes à mon cou\nsans me retourner.\n...\n\n\nEt voilà toute l'histoire.\n\n\nLa "
      - control:
          kind: set_colour
          colour: red
      - text: montagne louche
      - control:
          kind: reset_colour
      - text: ", c'est le "
      - control:
          kind: set_colour
          colour: blue
      - text: "mont\nSatori"
      - control:
          kind: reset_colour
      - text: ", à l'ouest du "
      - control:
          kind: set_colour
          colour: blue
      - text: "centre d'Hyrule"
      - control:
          kind: reset_colour
      - text: ".\n\n"
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "Et le chiot qui m'a sauvé la vie,"
      - control:
          kind: pause
          length: long
      - text: "\nc'est "
      - control:
          kind: set_colour
          colour: blue
      - text: Satori
      - control:
          kind: reset_colour
      - text: ".\n\nSi t'as ce qu'il faut dans le ventre, tu\npeux aller voir ce "
      - control:
          kind: set_colour
          colour: blue
      - text: bestiau
      - control:
          kind: reset_colour
      - text: " par toi-même."
  talk065:
    attributes: Npc_NorthHateru005
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Hein? "
      - control:
          kind: pause
          length: short
      - text: "Tu veux que j'te raconte encore\nmon histoire avec Satori?"
      - control:
          kind: choice
          choice_labels:
            - 6
            - 7
