    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "Tu n'oublies pas ma grande découverte,\nhein?\n\nSi j'étais un "
      - control:
          kind: set_colour
          colour: red
      - text: cheval squelette
      - control:
          kind: reset_colour
      - text: ", j'imagine\nque je ne sortirais que la nuit. Un peu\ncomme les "
      - control:
          kind: raw
          two_hundred_one:
            dynamic:
              - 0
              - len: 4
                field_2:
                  - 1
                  - 0
                  - 0
                  - 0
