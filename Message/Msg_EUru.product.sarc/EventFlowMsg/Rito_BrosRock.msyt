  Demo305_0_01:
    attributes: Npc_HighMountain035
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: Ух ты!
      - control:
          kind: pause
          length: short
      - text: " Сестренки, смотрите!"
  Demo305_0_02:
    attributes: Npc_HighMountain034
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 6
      - text: Ничего себе!
      - control:
          kind: pause
          length: short
      - text: " Это домик!"
  Demo305_0_03:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: А вот и нет!
      - control:
          kind: pause
          length: short
      - text: " Это же святилище!"
      - control:
          kind: pause
          length: short
      - text: "\nСтарейшина про них рассказывал!"
  Demo305_0_04:
    attributes: Npc_HighMountain033
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Классно!
      - control:
          kind: pause
          length: short
      - text: " Я и не знала, что\nв Гнезде Певчих есть такой\nсекрет!"
  Demo305_0_05:
    attributes: Npc_HighMountain031
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 19
      - text: Давайте маме расскажем!
  Demo305_0_06:
    attributes: Npc_HighMountain035
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Скорее!
  Demo305_0_07:
    attributes: Npc_HighMountain035
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Пока-пока!
  Demo305_0_08:
    attributes: Npc_HighMountain032
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: А давайте наперегонки!
