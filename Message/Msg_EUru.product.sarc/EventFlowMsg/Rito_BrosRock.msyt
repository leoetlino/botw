    attributes: Npc_HighMountain035
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 4
      - text: Кто последняя — курица!
      - control:
          kind: pause
          length: short
      - text: "\nНа старт, внимание, марш!"
  Npc_HighMountain031_R_001:
    attributes: Npc_HighMountain031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: Лю-лю-лю-у!♪ Лю-лю-лю-лю-у!♪
      - control:
          kind: pause
          length: short
      - text: "\nЛю-у-лю! Ля-ля! Лю-лю-лю-у!\nЛя-ля-ля-а!♪"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Вы прослушали вступление\nк «Возвращению героя после\nусмирения чудо-птицы»!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Благодарю за внимание!
  Npc_HighMountain031_R_002:
    attributes: Npc_HighMountain031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Мы поем о-о-очень старые песни!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Пением мы занимаемся у "
      - control:
          kind: set_colour
          colour: red
      - text: "Гнезда\nПевчих"
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: pause
          length: short
      - text: " Приходи, если захочешь\nпослушать!"
  Npc_HighMountain031_S01_001:
    attributes: Npc_HighMountain031
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 4
      - text: Лял-ля-а!♪ Лю-ля-ля-а!♪
      - control:
          kind: pause
          length: short
      - text: "\nЛя-ля-ля-а! Лю-лю! Лял-лял-л!\nЛя-ля-а!♪"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Вы прослушали первый куплет\n«Возвращения героя после\nусмирения чудо-птицы»!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Благодарю за внимание!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 19
      - text: Что?
      - control:
          kind: pause
          length: short
      - text: " Репетиция у "
      - control:
          kind: set_colour
          colour: red
      - text: "Гнезда\nПевчих"
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " Ничего я не забыла!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Генли хотела поесть "
      - control:
          kind: set_colour
          colour: blue
      - text: семги под соусом
      - control:
          kind: reset_colour
      - text: ",\nпоэтому мы с сестрами разделились\nи пошли покупать продукты."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я быстро нашла то, что нужно,\nи решила порепетировать тут,\nпока их жду."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Зря я не предупредила Киру...
      - control:
          kind: pause
          length: short
      - text: "\nНадо идти к ней репетировать!"
  Npc_HighMountain031_S01_002:
    attributes: Npc_HighMountain031
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Вот, дядя, возьми "
      - control:
          kind: set_colour
          colour: blue
      - text: табантскую пшеницу
      - control:
          kind: reset_colour
      - text: "\nи приготовь Генли вкусную "
      - control:
          kind: set_colour
          colour: blue
      - text: "семгу\nпод соусом"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Потому что она сказала, что не будет\nрепетировать, пока не покушает."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Наверняка это отговорка...
      - control:
          kind: pause
          length: short
      - text: "\nЛадно, всего хорошего!♪"
  Npc_HighMountain031_S01_003:
    attributes: Npc_HighMountain031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Я пойду к "
      - control:
          kind: set_colour
          colour: red
      - text: Гнезду Певчих
      - control:
          kind: reset_colour
      - text: ",\nкогда будут готовы остальные!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Лю-лю-лю-у!♪Гули-гули-гу-у!♪
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Кстати, Гули в "
      - control:
          kind: set_colour
          colour: red
      - text: «Песне Неба»
      - control:
          kind: reset_colour
      - text: ",\nа Коттс ловит рыбу у входа\nв деревню.♪"
