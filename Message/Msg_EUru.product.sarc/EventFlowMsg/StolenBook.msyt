          cancel_index: 1
          unknown: 6
  Npc_Kakariko012_S1_002:
    attributes: Npc_Kakariko016
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 20
      - text: А?
      - control:
          kind: pause
          length: short
      - text: " К-кто здесь?"
  Npc_Kakariko012_S1_003:
    attributes: Npc_Kakariko016
    contents:
