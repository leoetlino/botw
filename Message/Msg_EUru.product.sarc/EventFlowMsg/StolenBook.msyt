  Npc_Kakariko008_10:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: Кхе-кхе!
  Npc_Kakariko008_11:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: Я все же попалась...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - text: "Ах, эта сочная..."
