          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: Ну и молодежь пошла...
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
  Npc_Kakariko008_18:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 1
      - control:
          kind: text_size
          percent: 80
      - text: "Только никому не говори про морковь,\nладно?"
      - control:
          kind: pause
          length: short
      - text: " Я на тебя полагаюсь!"
      - control:
          kind: text_size
          percent: 100
  Npc_Kakariko008_19:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: И что?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: sound
          unknown:
            - 8
            - 1
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: text_size
          percent: 80
      - text: "Я же говорила: никто не\nдолжен знать про морковь!"
      - control:
          kind: text_size
          percent: 100
  Npc_Kakariko012_S1_001:
    attributes: Npc_Kakariko016
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Добро пожаловать "
      - control:
