      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: Да Импа мне как мать!
      - control:
          kind: pause
          length: short
      - text: " Она ведь мне\nсливовый сад подарила!"
      - control:
          kind: pause
          length: short
      - text: " Я бы никогда\nс ней так не поступила!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: set_colour
          colour: grey
      - control:
          kind: text_size
          percent: 80
      - text: Ну и молодежь пошла...
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: reset_colour
  Npc_Kakariko008_17:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: Опять за свое!
      - control:
          kind: pause
          length: short
      - text: " Чуть морковкой\nиз-за тебя не подавилась!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: Мне Импа как мать!
      - control:
          kind: pause
          length: short
      - text: " Она мне сливовый\nсад подарила!"
      - control:
          kind: pause
          length: short
      - text: " Я бы никогда с ней так\nне поступила!"
      - control:
