          kind: reset_colour
      - control:
          kind: text_size
          percent: 100
  Npc_Kakariko008_03:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - text: Кто тут?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "И зачем ты преследуешь старую\nбольную женщину, бандюган?"
      - control:
