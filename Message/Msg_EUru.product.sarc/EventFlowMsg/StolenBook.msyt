  Npc_Kakariko007_S1_001:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: "Кто же мог украсть "
      - control:
          kind: set_colour
          colour: blue
      - text: древнюю сферу
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Да, в дом кое-кто заходил,"
      - control:
          kind: pause
          length: short
      - text: " но это\nбыли местные."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
