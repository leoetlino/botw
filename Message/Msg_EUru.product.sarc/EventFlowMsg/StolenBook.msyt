          cancel_index: 1
          unknown: 6
  Npc_Kakariko007_S1_002_2:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: Я?
      - control:
          kind: pause
          length: short
      - text: " Ну я..."
      - control:
          kind: pause
          length: short
      - text: " это..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: Деревню...
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Точно!
      - control:
          kind: pause
          length: short
      - text: " Деревню обхожу!"
      - control:
          kind: pause
          length: short
      - text: " Тут\nведь где-то бандит прячется!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Хотя, наверное, зря я оставил Кадо\nодного на посту."
      - control:
          kind: pause
          length: short
      - text: " Прошу прощения —\nя лучше обратно пойду."
  Npc_Kakariko007_S1_002_3:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 4
      - text: "Если будешь себя по ночам странно\nвести, тебя могут и за вора принять."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Будь осторожен.
  Npc_Kakariko007_S1_002_4:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 22
      - text: Линк?!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: Что ты здесь делаешь?
      - control:
          kind: choice
          choice_labels:
            - 7
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Kakariko007_S1_002_5:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 16
      - text: "Если будешь разгуливать по ночам,\nда еще в таком месте, тебя могут\nи за вора принять."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Ха-ха...
  Npc_Kakariko007_S1_002_6:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 21
      - text: Что я делаю?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 16
      - text: Ну я...
      - control:
          kind: pause
          length: short
      - text: " просто..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: Точно!
      - control:
          kind: pause
          length: short
      - text: " Я просто гуляю."
      - control:
          kind: pause
          length: short
      - text: " Даже не\nзаметил, что так далеко зашел."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Пойду, пожалуй, обратно."
  Npc_Kakariko007_S1_002_7:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 4
      - text: "Линк, это ты!"
  Npc_Kakariko007_S1_Near001:
    attributes: Npc_Kakariko007
    contents:
      - text: "..."
  Npc_Kakariko007_S1_Near002:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 2
            - 14
      - text: Как они могли!..
  Npc_Kakariko007_S1_Near003:
    attributes: Npc_Kakariko007
    contents:
      - text: Надо что-то делать...
  Npc_Kakariko007_S1_Near004:
    attributes: Npc_Kakariko007
    contents:
