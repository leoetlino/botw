  Npc_Kakariko007_S1_Near008:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 19
      - text: Что это было?
  Npc_Kakariko007_S1_Near009:
    attributes: Npc_Kakariko007
    contents:
      - text: "Наверное, показалось..."
  Npc_Kakariko007_S1_Near010:
