      - control:
          kind: sound
          unknown:
            - 10
            - 20
      - control:
          kind: text_size
          percent: 125
      - text: "Ишь ты, хулиган какой!"
      - control:
          kind: text_size
          percent: 100
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 5
            - 1
      - text: "Ладно, чего я на тебя время трачу..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: set_colour
          colour: grey
      - text: Ночь коротка...
