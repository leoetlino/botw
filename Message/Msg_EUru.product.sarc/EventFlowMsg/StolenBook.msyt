      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "И вообще, что это еще за вопросы!"
      - control:
          kind: pause
          length: short
      - text: "\nЯ по ночам что хочу, то и делаю."
  Npc_Kakariko008_07:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 9
      - text: Ну и напугал ты меня!
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: text_size
          percent: 80
      - control:
          kind: set_colour
          colour: grey
      - text: "Может, он все знает?"
      - control:
          kind: pause
          length: short
