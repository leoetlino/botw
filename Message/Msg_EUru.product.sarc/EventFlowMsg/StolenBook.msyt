  Npc_Kakariko008_08:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: Вот я и попалась...
  Npc_Kakariko008_09:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 6
      - text: Ты про ту штуку в доме Импы?
