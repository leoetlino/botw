  Npc_Kakariko007_S1_002_1:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 22
      - text: "Ой, Линк!"
      - control:
          kind: pause
          length: short
      - text: " Что ты здесь\nделаешь в такое время?"
      - control:
