entries:
  "0000":
    attributes: ""
    contents:
      - text: Беру!
  "0001":
    attributes: ""
    contents:
      - text: До свидания
  "0002":
    attributes: ""
    contents:
      - text: Научите!
  "0003":
    attributes: ""
    contents:
      - text: Мне пора
  "0004":
    attributes: ""
    contents:
      - text: Секреты защиты...
  "0005":
    attributes: ""
    contents:
      - text: Прыжок-уклонение
  "0006":
    attributes: ""
    contents:
      - text: Отличный блок
  "0007":
    attributes: ""
    contents:
      - text: Я все видел
  "0008":
    attributes: ""
    contents:
      - text: Я ничего не видел...
  "0009":
    attributes: ""
    contents:
      - text: Прыжок вбок...
  "0010":
    attributes: ""
    contents:
      - text: Сальто назад...
  "0013":
    attributes: ""
    contents:
      - text: Готовит?
  Near00:
    attributes: Npc_Kakariko006
    contents:
      - text: Тыкву хочешь?
  Near01:
    attributes: Npc_Kakariko006
    contents:
      - text: Опять дождь!
  Near02:
    attributes: Npc_Kakariko006
    contents:
      - text: Хо-хо-хо!
  Near03:
    attributes: Npc_Kakariko006
    contents:
      - text: Ха!
  Near04:
    attributes: Npc_Kakariko006
    contents:
      - text: Нужно больше тыкв...
  Near05:
    attributes: Npc_Kakariko006
    contents:
      - text: Чудесный день!
  Near06:
    attributes: Npc_Kakariko006
    contents:
      - text: Дождь что надо!
  Near07:
    attributes: Npc_Kakariko006
    contents:
      - text: Есть хочется...
  Near08:
    attributes: Npc_Kakariko006
    contents:
      - text: Хорошо на душе!
  Near09:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Любимые тыквочки...
  Near10:
    attributes: Npc_Kakariko006
    contents:
      - text: Пора домой...
  Near11:
    attributes: Npc_Kakariko006
    contents:
      - text: Устал сегодня...
  SupriseHouse:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 19
            - 205
      - text: Ох!
  SupriseHouse_00:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "А, это ты..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Не пугай так!
      - control:
          kind: pause
          length: short
      - text: " Здесь только\nмоя старушка бывает."
  SupriseHouse_01:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "..."
  Talk00:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: "Понравилась "
      - control:
          kind: set_colour
          colour: blue
      - text: латная тыква
      - control:
          kind: reset_colour
      - text: "?"
  Talk01:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " А ты разбираешься в тыквах!"
  Talk02:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Вот и кончились мои тыквы."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Я ведь их своими руками выращивал.
      - control:
          kind: pause
          length: short
      - text: "\nРад, что не зря время тратил, и они\nтебе пригодились."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Следующий урожай будет\nеще лучше, вот увидишь."
  Talk03:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: Но она же такая вкусная...
  Talk04:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Хочешь "
      - control:
          kind: set_colour
          colour: blue
      - text: латную тыкву
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: "\nЯ как раз в огород иду, потерпи\nнемного."
  Talk05:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 25
      - text: "Извини, я ужинать собрался."
      - control:
          kind: pause
          length: short
      - text: " Если\nты насчет тыкв, приходи завтра."
  Talk06:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: "Одна "
      - control:
          kind: raw
          two_hundred_one:
            one_field:
              - 4
              - field_1: 0
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: variable
          variable_kind: 11
          name: PlacedItemName
      - control:
          kind: reset_colour
      - text: " —\n"
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 16
          name: PlacedItemPrice
      - control:
          kind: localisation
          localisation_kind: plural
          options:
            - " рупия"
            - " рупий"
            - " рупии"
      - control:
          kind: reset_colour
      - text: ". Берешь?"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk07:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: sound2
          unknown:
            - 21
            - 205
      - text: "Блюда, которые моя жена готовит\nиз "
      - control:
          kind: set_colour
          colour: blue
      - text: латных тыкв
      - control:
          kind: reset_colour
      - text: ", просто отменные!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Никакая морковь и рядом не валялась.
  Talk08:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: Ты чего тут делаешь?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Вообще-то этот сливовый сад много\nзначит для моей жены, так что ты\nпоаккуратнее!"
  Talk09:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 17
            - 205
      - text: Кто здесь?
  Talk10:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ты чего по темноте шастаешь?
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я вот по ночам бегаю, чтобы держать\nноги в форме, потому что крепкие\nноги — залог хорошей защиты."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Только путем упорных тренировок\nможно овладеть секретами защиты!"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk11:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: Хо-хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Мы, шиика, ценим\nдух больше всего остального."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Потому что одной только грубой\nсилой нельзя защитить тех, кто\nтебе дорог."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Как бы хорошо ты ни дрался,\nвсегда помни об этом."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
            - 6
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk12:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Давным-давно в нашей деревне жил\nумелый воин."
      - control:
          kind: pause
          length: short
      - text: " Это для него построено\nсвятилище над деревней."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Говорят, если пройти его испытание,\nовладеешь секретами защиты."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "Только никто не знает, в чем\nсостоит это самое испытание..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Но я все равно регулярно тренируюсь.
      - control:
          kind: pause
          length: short
      - text: "\nХочу быть готовым пройти испытание\nв любой момент."
  Talk13:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Есть у нас один ненормальный,\nкоторый болтает, что лучшая\nзащита — нападение."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "А я вот считаю, что сильнее тот,\nкто владеет искусством защиты."
  Talk14:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Просто нацелься на\nврага и прыгни вбок."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Но прыгать надо красиво — так,\nчтобы смотреть было приятно."
  Talk16:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 23
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Нечасто у нас в деревне\nтакого молодца увидишь!"
  Talk18:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - control:
          kind: set_colour
          colour: blue
      - text: Латные тыквы
      - control:
          kind: reset_colour
      - text: " укрепляют тело."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: Моя старушка каждый день их готовит.
      - control:
          kind: choice
          choice_labels:
            - 13
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk19:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Шучу!"
      - control:
          kind: pause
          length: short
      - text: " Просто ты такой\nсерьезный, что я не удержался."
  Talk20:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 20
            - 205
      - text: "Ох, это ты!"
      - control:
          kind: pause
          length: short
      - text: " Чего тебе?"
      - control:
          kind: choice
          choice_labels:
            - 7
            - 8
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk21:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - control:
          kind: set_colour
          colour: blue
      - text: Латные тыквы
      - control:
          kind: reset_colour
      - text: " — гордость "
      - control:
          kind: set_colour
          colour: blue
      - text: "деревни\nКакарико"
      - control:
          kind: reset_colour
      - text: "!"
      - control:
          kind: pause
          length: short
      - text: " Всю душу в них вкладываю,\nпотому-то они такие вкусные."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Если нужно еще, приходи\nко мне на огород."
  Talk22:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Поздно уже.
      - control:
          kind: pause
          length: short
      - text: " Пора домой."
  Talk23:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Вот ведь старый\nчудак!"
      - control:
          kind: pause
          length: short
      - text: " Бегает и кричит..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: «Лучшая защита — нападение!»
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Объелся своей моркови...
  Talk24:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 32
      - text: "В молодости я знаешь как быстро по\nдеревне бегал!"
      - control:
          kind: pause
          length: short
      - text: " Меня так и звали —\nОлкин-скороход."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Но сколько ни бегай, от\nстарости не убежишь..."
  Talk25:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Если хочешь научиться чему-нибудь\nеще, спрашивай, не стесняйся."
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
            - 6
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk26:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: Ты чего по темноте шастаешь?
      - control:
          kind: pause
          length: short
      - text: "\nДа еще и в дождь..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я вот по ночам бегаю, чтобы держать\nноги в форме, потому что крепкие\nноги — залог хорошей защиты."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Только путем упорных тренировок\nможно овладеть секретами защиты!"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk27:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: "Кажется, я попался..."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Мы со Стином давно не ладим.
      - control:
          kind: pause
          length: short
      - text: "\nА раньше даже охотились вместе."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "Я, значит, подкрадываюсь к зверю,\nа он как побежит!.."
      - control:
          kind: pause
          length: short
      - text: " Да еще и орет:"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 8
            - 7
      - text: «Лучшая защита — нападение!»
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "Зверь, конечно, сразу в кусты —\nи только его и видели."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "И тыквы мои он не хочет\nпродавать в "
      - control:
          kind: set_colour
          colour: blue
      - text: магазине
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: Что я ему сделал-то?
      - control:
          kind: pause
          length: short
      - text: " Досада..."
  Talk28:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: Хо-хо!
      - control:
          kind: pause
          length: short
      - text: " Вот и чудненько!"
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: "В общем, я сейчас..."
      - control:
          kind: pause
          length: short
      - text: " э-э, немного\nзанят."
      - control:
          kind: pause
          length: short
      - text: " Потом поболтаем."
  Talk29:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 22
      - text: "А ты, часом, не "
      - control:
          kind: set_colour
          colour: blue
      - text: бандит Ига
      - control:
          kind: reset_colour
      - text: "?"
      - control:
          kind: pause
          length: short
      - text: " А?"
  Talk30:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: "Ладно, теперь я понимаю,\nчто так делать нельзя."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - text: Больше не буду грядки портить.
  Talk31:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Прыжок — один из базовых элементов\nзащиты."
      - control:
          kind: pause
          length: short
      - text: " Тебе стоит знать о двух видах\nпрыжков."
      - control:
          kind: choice
          choice_labels:
            - 9
            - 10
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk32:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "Нацелься на врага и отпрыгни от него,\nчтобы выполнить сальто назад."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: Хо-хо-хо!
      - control:
          kind: pause
          length: short
      - text: " В молодости я так скакал,\nчто даже ты позавидовал бы!"
  Talk34:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - control:
          kind: set_colour
          colour: blue
      - text: Бандитами Ига
      - control:
          kind: reset_colour
      - text: " мы называем тех,\nкто перешел на сторону "
      - control:
          kind: set_colour
          colour: red
      - text: Ганона
      - control:
          kind: reset_colour
      - text: "."
      - control:
          kind: raw
          zero:
            four:
              field_1: 0
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - control:
          kind: sound2
          unknown:
            - 15
            - 205
