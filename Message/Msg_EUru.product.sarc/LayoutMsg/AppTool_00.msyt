group_count: 17
atr1_unknown: 0
entries:
  "0000":
    contents:
      - text: Создает из воды блоки льда.
  "0001":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Лед очень прочный и не тает. По нему можно "
      - control:
          kind: set_colour
          colour: orange
      - text: ходить
      - control:
          kind: set_colour
          colour: light_green4
      - text: "\nили использовать его как "
      - control:
          kind: set_colour
          colour: orange
      - text: заслон
      - control:
          kind: set_colour
          colour: light_green4
      - text: ". Чтобы "
      - control:
          kind: set_colour
          colour: orange
      - text: разбить
      - control:
          kind: set_colour
          colour: light_green4
      - text: " лед,\nеще раз используйте на нем Крионис."
      - control:
          kind: reset_colour
  "0002":
    contents:
      - text: FifthItem
  "0003":
    contents:
      - text: Снимает окружающую действительность.
  "0004":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Снимки сохраняются в альбоме. Если что-то\nснято впервые, снимок автоматически добавится\nи в атлас Хайрула."
      - control:
          kind: reset_colour
  "0005":
    contents:
      - text: SixthItem
  "0006":
    contents:
      - text: "Распознайте amiibo, и что-нибудь появится."
  "0007":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Каждый раз создаются случайные предметы.\nКаждый amiibo можно использовать лишь один\nраз в день."
      - control:
          kind: reset_colour
  "0008":
    contents:
      - text: SeventhItem
  "0009":
    contents:
      - text: Перемещает металлические предметы.
  "0010":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Поймав в магнитное поле металлический\nпредмет, вы можете "
      - control:
          kind: set_colour
          colour: orange
      - text: "поднимать и свободно\nперемещать его"
      - control:
          kind: set_colour
          colour: light_green4
      - text: "."
      - control:
          kind: reset_colour
  "0011":
    contents:
      - text: ThirdItem
  "0012":
    contents:
      - text: Можно взрывать на расстоянии.
  "0013":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Взрыв "
      - control:
          kind: set_colour
          colour: orange
      - text: разрушает
      - control:
          kind: set_colour
          colour: light_green4
      - text: " предметы и "
      - control:
          kind: set_colour
          colour: orange
      - text: наносит урон
      - control:
          kind: set_colour
          colour: light_green4
      - text: " монстрам.\nБомбы бывают кубические и круглые — выбирайте по\nситуации."
      - control:
          kind: reset_colour
  "0014":
    contents:
      - text: Можно взрывать на расстоянии.
  "0015":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Улучшенная бомба: мощнее, быстро перезаряжается.\nВзрыв "
      - control:
          kind: set_colour
          colour: orange
      - text: разрушает
      - control:
          kind: set_colour
          colour: light_green4
      - text: " предметы и "
      - control:
          kind: set_colour
          colour: orange
      - text: наносит урон
      - control:
          kind: set_colour
          colour: light_green4
      - text: " монстрам.\nБомбы бывают кубические и круглые — выбирайте по\nситуации."
      - control:
          kind: reset_colour
  "0016":
    contents:
      - text: FirstItem
  "0017":
    contents:
      - text: SecondItem
  "0018":
    contents:
      - text: Останавливает объекты во времени.
  "0019":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Бейте по остановленному во времени объекту: он будет\nнакапливать"
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: orange
      - text: энергию
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: light_green4
      - text: "ударов и высвободит ее, как только\nдля него возобновится ход времени. Таким образом можно"
      - control:
          kind: reset_colour
      - text: "\n"
      - control:
          kind: set_colour
          colour: orange
      - text: сдвинуть с места даже огромный объект
      - control:
          kind: reset_colour
      - control:
          kind: set_colour
          colour: light_green4
      - text: "."
      - control:
          kind: reset_colour
  "0020":
    contents:
      - text: Останавливает объекты во времени.
  "0021":
    contents:
      - control:
          kind: set_colour
          colour: light_green4
      - text: "Улучшенный Стазис действует даже на монстров.\nБейте по остановленному во времени объекту, и он\nбудет накапливать"
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: orange
      - text: энергию
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: light_green4
      - text: "ударов. Таким образом\nможно"
      - control:
          kind: reset_colour
      - text: " "
      - control:
          kind: set_colour
          colour: orange
      - text: сдвинуть с места даже огромный объект
      - control:
          kind: reset_colour
      - control:
          kind: set_colour
          colour: light_green4
      - text: "."
      - control:
          kind: reset_colour
  "0022":
    contents:
      - text: FourthItem
  "0023":
    contents:
      - text: "Руны отсутствуют.\nИспользование невозможно."
