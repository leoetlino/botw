      - control:
          kind: reset_colour
      - text: "……\n……\n\n"
      - control:
          kind: sound
          unknown:
            - 10
            - 0
      - text: 你是……
      - control:
          kind: pause
          length: longer
      - text: "林克！\n\n"
      - control:
          kind: sound
          unknown:
            - 6
            - 1
      - text: 在100年前敗給
      - control:
