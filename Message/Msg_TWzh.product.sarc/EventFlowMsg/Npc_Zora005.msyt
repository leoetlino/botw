      - text: "令人手癢咋啦！\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 你隨後再跟上吧，
      - control:
          kind: pause
          length: short
      - text: "\n我會在"
      - control:
          kind: set_colour
