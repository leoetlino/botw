          unknown: 6
  talk22:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: 那
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾的長相
      - control:
          kind: reset_colour
      - text: 一事……
      - control:
          kind: pause
          length: longer
      - text: "\n就拜託你了。"
  talk23:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: 用攀登瀑布的方式前往
      - control:
          kind: set_colour
          colour: blue
      - text: 雷獸山
      - control:
          kind: reset_colour
      - text: "比較方便。\n\n只要穿上"
      - control:
          kind: set_colour
          colour: blue
      - text: 卓拉鎧甲
      - control:
          kind: reset_colour
      - text: ，即使是海利亞人，
      - control:
          kind: pause
          length: short
      - text: "\n也能瞬間從瀑布下方攀登至上方哦。"
  talk24:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "……\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 19
      - text: 啊！不過
      - control:
          kind: set_colour
          colour: blue
      - text: 卓拉鎧甲
      - control:
          kind: reset_colour
      - control:
          kind: pause
          length: short
      - text: "是王族閨女\n要送給未來女婿的特別物品……\n有沒有辦法跟別人借呢？"
  talk25:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 12
      - text: 應該沒那麼剛好
      - control:
          kind: pause
          length: short
      - text: "\n會有……"
      - control:
          kind: set_colour
          colour: blue
      - text: 卓拉鎧甲
      - control:
          kind: reset_colour
      - text: 吧……
      - control:
          kind: pause
          length: short
      - text: "\n只要穿上那個，"
      - control:
          kind: pause
          length: short
      - text: "\n就能瞬間攀登至瀑布上方了……"
  talk26:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: 林克先生……
      - control:
          kind: pause
          length: longer
      - text: "\n請問"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾的長相
      - control:
