          cancel_index: 1
          unknown: 6
  talk15:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: 太好了……
      - control:
          kind: pause
          length: longer
      - text: "\n我本來還在煩惱該怎麼辦呢。 "
  talk16:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 12
      - text: 說成這樣還是不懂嗎……
      - control:
          kind: pause
          length: longer
      - text: "\n那我就用小朋友也聽得懂的方式來說吧。"
  talk17:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: 有看到那座
      - control:
          kind: set_colour
          colour: blue
      - text: 雷獸山
      - control:
          kind: reset_colour
      - text: 嗎？
      - control:
          kind: pause
          length: longer
      - text: "\n長得尖尖的對吧～？\n那邊住著"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾
      - control:
          kind: reset_colour
      - text: "，\n是一種長有紅毛的怪物哦！很可怕吧～！\n我無論如何都想看看\n"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾的長相
      - control:
          kind: reset_colour
      - text: "♪\n所以找其他地方的也可以哦。\n最重要的是能看到"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾
      - control:
          kind: reset_colour
      - text: "的長相！\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: 不過可別把本尊帶來哦♪
      - control:
          kind: pause
          length: longer
      - text: "\n因為那很恐怖呀～♪\n所以"
      - control:
          kind: set_colour
          colour: red
      - text: 不是本尊也沒關係
      - control:
          kind: reset_colour
      - text: ，
      - control:
          kind: pause
          length: short
      - text: "\n能讓我看看"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾的長相
      - control:
          kind: reset_colour
      - text: "嗎～？\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 6
      - text: ……這樣你懂了嗎？
  talk19:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 16
      - text: 其實在
      - control:
          kind: set_colour
          colour: blue
      - text: 雷獸山
      - control:
          kind: reset_colour
      - text: 的前端，
      - control:
          kind: pause
          length: short
      - text: "\n有個叫"
      - control:
          kind: set_colour
          colour: blue
      - text: 嘗試海角
      - control:
          kind: reset_colour
      - text: "的突出懸崖。\n但卓拉領地有個可惡的傢伙，"
      - control:
          kind: pause
          length: short
      - text: "\n推薦大家體驗從那邊跳入東部的儲水湖的娛樂……\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: 所以我想說只要將
      - control:
          kind: set_colour
          colour: blue
      - text: 雷獸山
      - control:
          kind: reset_colour
      - text: 的怪物……
      - control:
          kind: pause
          length: short
      - text: "\n也就是"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾的長相
      - control:
          kind: reset_colour
      - text: "口耳相傳……\n應該就不會有人接近"
      - control:
          kind: set_colour
          colour: blue
      - text: 嘗試海角
      - control:
          kind: reset_colour
      - text: 了吧。
      - control:
          kind: pause
          length: short
      - text: "\n林克先生……"
  talk20:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: 方法交給你決定。
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: set_colour
          colour: red
      - text: 萊尼爾的長相
      - control:
          kind: reset_colour
      - text: 一事……
      - control:
          kind: pause
          length: short
      - text: 就麻煩你了。
  talk21:
    attributes: Npc_Zora015
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 19
      - text: 該不會我……
      - control:
          kind: pause
          length: longer
      - text: "\n沒說到前往"
      - control:
          kind: set_colour
          colour: blue
      - text: 雷獸山
      - control:
          kind: reset_colour
      - text: 的方法吧……
      - control:
