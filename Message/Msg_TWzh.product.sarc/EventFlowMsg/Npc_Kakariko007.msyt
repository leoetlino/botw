  "0005":
    attributes: ""
    contents:
      - text: 請慢聊
  Near00:
    attributes: Npc_Kakariko007
    contents:
      - text: 女性的哭聲……
  SupriseHouse:
    attributes: Npc_Kakariko007
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: 是、是誰！
      - control:
          kind: pause
          length: longer
      - text: "\n躲在那邊做什麼！"
      - control:
          kind: pause
          length: longer
      - text: "\n…"
      - control:
          kind: pause
          length: short
      - text: …
      - control:
          kind: pause
