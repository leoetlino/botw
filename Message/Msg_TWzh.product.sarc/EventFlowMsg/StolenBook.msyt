            - 8
            - 3
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Npc_Kakariko008_05:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 4
            - 44
      - text: 咳咳！
  Npc_Kakariko008_06:
    attributes: Npc_Kakariko008
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 20
